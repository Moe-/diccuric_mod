		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8336]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8337]
				subType=enum:0
				text=string:Ich rate dir trotzdem, allem nachzugehen, was dir etwas über deinen Vater sagen kann. Selbst diesen Gerüchten.
				name=string:DIA_DIC_Alwin_Vater_01_01.WAV
			[]
