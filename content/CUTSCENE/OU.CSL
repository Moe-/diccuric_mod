	[% zCCSBlock 0 6511]
		blockName=string:DIA_DIC_Lagariman_Wer_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6512]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6513]
				subType=enum:0
				text=string:Ich bin Lagariman, ein Freund deines Vaters.
				name=string:DIA_DIC_Lagariman_Wer_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6514]
		blockName=string:DIA_DIC_Lagariman_Wer_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6515]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6516]
				subType=enum:0
				text=string:Du kennst meinen Vater?
				name=string:DIA_DIC_Lagariman_Wer_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6517]
		blockName=string:DIA_DIC_Lagariman_Wer_07_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6518]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6519]
				subType=enum:0
				text=string:Ja, ich war sein Gefolgsmann, bevor er Etain verlassen hat, um dem König Gatyahs zu dienen. Als ich ihn vor ein paar Monaten in der Hauptstadt aufsuchte hörte ich, dass dein Vater angeblich die Stadt verlassen hat. Keiner wusste wo er hingezogen war. Ich ahnte was wirklich geschehen war.
				name=string:DIA_DIC_Lagariman_Wer_07_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6520]
		blockName=string:DIA_DIC_Lagariman_Wer_15_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6521]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6522]
				subType=enum:0
				text=string:Was taten die Magier einst in Etain, und warum war mein Vater früher hier ?
				name=string:DIA_DIC_Lagariman_Wer_15_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6523]
		blockName=string:DIA_DIC_Lagariman_Wer_07_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6524]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6525]
				subType=enum:0
				text=string:Bevor dieses Gebiet dem Erdboden gleichgemacht wurde, lebte hier der Orden der Madrimier. Dein Vater suchte hier nach Artefakten. Sie sollten helfen, das Bestehen des Guten in der Welt zu sichern. Er hoffte, mit ihnen den König zu unterstützen. Denn er ahnte Unheil, welches auf das Reich zukommen würde.
				name=string:DIA_DIC_Lagariman_Wer_07_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6526]
		blockName=string:DIA_DIC_Lagariman_Was_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6527]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6528]
				subType=enum:0
				text=string:Was tust du hier?
				name=string:DIA_DIC_Lagariman_Was_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6529]
		blockName=string:DIA_DIC_Lagariman_Was_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6530]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6531]
				subType=enum:0
				text=string:Ich habe mich auf eine lange Reise begeben, um deinen Vater zu suchen. Meine Reise führte mich hierhin, an dieses Grab.
				name=string:DIA_DIC_Lagariman_Was_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6532]
		blockName=string:DIA_DIC_Lagariman_Was_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6533]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6534]
				subType=enum:0
				text=string:Hier soll das Grab meines Vaters sein?
				name=string:DIA_DIC_Lagariman_Was_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6535]
		blockName=string:DIA_DIC_Lagariman_Was_07_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6536]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6537]
				subType=enum:0
				text=string:Ja. Und es war Berengar, der ihn tötete!
				name=string:DIA_DIC_Lagariman_Was_07_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6538]
		blockName=string:DIA_DIC_Lagariman_Was_15_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6539]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6540]
				subType=enum:0
				text=string:Dann ist es also wahr.
				name=string:DIA_DIC_Lagariman_Was_15_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6541]
		blockName=string:DIA_DIC_Lagariman_Was_07_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6542]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6543]
				subType=enum:0
				text=string:Berengar hat einst ein Komplott gegen den König vorbereitet. Dein Vater berichtete dies dem Herrscher Gatyahs. Damals waren Berengar und der König noch Gefährten. Als der König merkte, dass sein Freund ihn töten will, entschloss er, ihn zu zwingen, das Reich für immer zu verlassen. Doch bevor Berengar das Königreich verlies, rächte er sich an deinem Vater.
				name=string:DIA_DIC_Lagariman_Was_07_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6544]
		blockName=string:DIA_DIC_Lagariman_Landstreicher_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6545]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6546]
				subType=enum:0
				text=string:Du bist einer der Landstreicher. Ich werde nicht darauf warten, bis du dein Messer ziehst. Kämpfe!
				name=string:DIA_DIC_Lagariman_Landstreicher_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6547]
		blockName=string:DIA_DIC_Lagariman_Landstreicher_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6548]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6549]
				subType=enum:0
				text=string:Mein Name ist Lagariman und ich bin ein Freund deines Vaters.
				name=string:DIA_DIC_Lagariman_Landstreicher_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6550]
		blockName=string:DIA_DIC_Lagariman_Woher_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6551]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6552]
				subType=enum:0
				text=string:Woher weißt du, dass ich Ardaric bin?
				name=string:DIA_DIC_Lagariman_Woher_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6553]
		blockName=string:DIA_DIC_Lagariman_Woher_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6554]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6555]
				subType=enum:0
				text=string:Barem sagte, dass er dich kennen gelernt hat. Ich versuchte dich zu treffen, doch das Lager Berengars war zu gut bewacht. Also sandte ich mit Hilfe der Magie einem Arbeiter, den ich draußen schlafend vorm Lager fand, einen Traum.
				name=string:DIA_DIC_Lagariman_Woher_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6556]
		blockName=string:DIA_DIC_Lagariman_Woher_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6557]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6558]
				subType=enum:0
				text=string:Als der Arbeiter erwachte, erinnerte er sich daran, dass er ein Gerücht gehört hatte. Ich wusste, dass das Gerücht um deinen Vater herumerzählt wird, und dass es irgendwann zu dir dringen würde. Und jetzt bist du hier.
				name=string:DIA_DIC_Lagariman_Woher_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6559]
		blockName=string:DIA_DIC_Lagariman_Kampf_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6560]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6561]
				subType=enum:0
				text=string:Ich glaube dir.nicht. Kämpfe!
				name=string:DIA_DIC_Lagariman_Kampf_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6562]
		blockName=string:DIA_DIC_Lagariman_Tot_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6563]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6564]
				subType=enum:0
				text=string:Mein Vater ist tot. Berengar ist mein neuer Vater.
				name=string:DIA_DIC_Lagariman_Tot_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6565]
		blockName=string:DIA_DIC_Lagariman_Tot_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6566]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6567]
				subType=enum:0
				text=string:Deine Worte sind, als wolltest du dich selbst mit einem Schwert verletzen. Hier, da liegt dein Vater! Genau vor dir.  Tot, in einem unwürdigen Grab verscharrt - Emmeran, dein Vater, von Berengar ermordet.
				name=string:DIA_DIC_Lagariman_Tot_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6568]
		blockName=string:DIA_DIC_Lagariman_Nichts_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6569]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6570]
				subType=enum:0
				text=string:Ich will davon nichts wissen.
				name=string:DIA_DIC_Lagariman_Nichts_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6571]
		blockName=string:DIA_DIC_Lagariman_Ueberfuehrt_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6572]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6573]
				subType=enum:0
				text=string:Aber ich verstehe nicht warum Berengar die Leiche hier nach Etain mitgenommen hat.
				name=string:DIA_DIC_Lagariman_Ueberfuehrt_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6574]
		blockName=string:DIA_DIC_Lagariman_Ueberfuehrt_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6575]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6576]
				subType=enum:0
				text=string:Der Tod deines Vaters hätte den König erzürnt. Berengar wäre verfolgt worden, und die Garde des Königs hätte binnen weniger Tage Berengar gefunden und gerichtet. Doch ohne die Leiche deines Vaters, weiß niemand, was geschehen ist.
				name=string:DIA_DIC_Lagariman_Ueberfuehrt_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6577]
		blockName=string:DIA_DIC_Lagariman_Ueberfuehrt_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6578]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6579]
				subType=enum:0
				text=string:Als ich nach Gatyah kam, galt dein Vater als verschwunden, aber nicht als tot. Ich vermute aber, dass es noch einen anderen Grund dafür gibt, dass Berengar den Leichnam deines Vaters nach Etain überführt hat. Doch bin ich mir noch nicht sicher genug, um darüber etwas zu sagen.
				name=string:DIA_DIC_Lagariman_Ueberfuehrt_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6580]
		blockName=string:DIA_DIC_Lagariman_Glaube_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6581]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6582]
				subType=enum:0
				text=string:Ich glaube dir. Aber was soll ich jetzt tun?
				name=string:DIA_DIC_Lagariman_Glaube_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6583]
		blockName=string:DIA_DIC_Lagariman_Glaube_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6584]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6585]
				subType=enum:0
				text=string:Du hast deine Heimat verloren und deine Familie. Berengar, an den du glaubtest, hat dich hintergangen. Ich fühle mich dazu berufen, dir beizustehen. Deshalb will ich dich den Weg lehren, den dein Vater beschritten hat. Ich werde dich leiten und dir bei den Prüfungen beistehen. Den Prüfungen, die dir deine wahre Stärke offenbaren werden: die Kraft der Magie.
				name=string:DIA_DIC_Lagariman_Glaube_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6586]
		blockName=string:DIA_DIC_Lagariman_Glaube_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6587]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6588]
				subType=enum:0
				text=string:Was muss ich tun?
				name=string:DIA_DIC_Lagariman_Glaube_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6589]
		blockName=string:DIA_DIC_Lagariman_Glaube_07_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6590]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6591]
				subType=enum:0
				text=string:Du wirst drei Prüfungen bestehen müssen. Die Prüfung des Körpers, des Geistes und der Seele. In jeder dieser Prüfung kannst du genau das verlieren was du zu gewinnen suchst.
				name=string:DIA_DIC_Lagariman_Glaube_07_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6592]
		blockName=string:DIA_DIC_Lagariman_Glaube_07_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6593]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6594]
				subType=enum:0
				text=string:Willst du dich diesen Gefahren aussetzen? Oder willst du dir vielleicht noch etwas Zeit lassen für diese Herausforderungen? Wenn du dich aber entscheidest gibt es kein zurück mehr.
				name=string:DIA_DIC_Lagariman_Glaube_07_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6595]
		blockName=string:DIA_DIC_Lagariman_Anfangen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6596]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6597]
				subType=enum:0
				text=string:Ja!
				name=string:DIA_DIC_Lagariman_Anfangen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6598]
		blockName=string:DIA_DIC_Lagariman_Anfangen_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6599]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6600]
				subType=enum:0
				text=string:Gut dann beginnen wir mit der Prüfung des Körpers.
				name=string:DIA_DIC_Lagariman_Anfangen_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6601]
		blockName=string:DIA_DIC_Lagariman_Anfangen_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6602]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6603]
				subType=enum:0
				text=string:Was muss ich tun?
				name=string:DIA_DIC_Lagariman_Anfangen_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6604]
		blockName=string:DIA_DIC_Lagariman_Anfangen_07_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6605]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6606]
				subType=enum:0
				text=string:Zwei Dinge sind zu tun, um diese Prüfung zu bestehen. Du musst einen Leitwolf und einen Ork töten.  Für diese Aufgabe ist es dir erlaubt zwei magische Hilfsmittel zu verwenden. Aber du darfst keinerlei Waffen wie Schwerter, Messer, Speere, gebrauchen.
				name=string:DIA_DIC_Lagariman_Anfangen_07_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6607]
		blockName=string:DIA_DIC_Lagariman_Zeit_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6608]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6609]
				subType=enum:0
				text=string:Ich brauche noch Zeit.... es mir zu überlegen.
				name=string:DIA_DIC_Lagariman_Zeit_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6610]
		blockName=string:DIA_DIC_Lagariman_Zeit_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6611]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6612]
				subType=enum:0
				text=string:Dann komme wieder wenn du bereit bist. Ich werde hier noch einige Zeit verweilen. Wirst du aber binnen eines Monats nicht wieder hier erscheinen, so nehme ich dies als Zeichen, dass du dich für einen anderen Weg entschieden hast.
				name=string:DIA_DIC_Lagariman_Zeit_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6613]
		blockName=string:DIA_DIC_Lagariman_Lernen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6614]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6615]
				subType=enum:0
				text=string:Was kann ich durch diese Prüfungen lernen? Ich haben einen Körper, einen Geist und eine Seele. Warum also sollte ich erwerben, was ich schon mein Eigen nenne?
				name=string:DIA_DIC_Lagariman_Lernen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6616]
		blockName=string:DIA_DIC_Lagariman_Lernen_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6617]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6618]
				subType=enum:0
				text=string:Was du dein Eigen nennst, würde ein Weiser nicht eine Sekunde lang besitzen wollen.
				name=string:DIA_DIC_Lagariman_Lernen_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6619]
		blockName=string:DIA_DIC_Lagariman_Lernen_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6620]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6621]
				subType=enum:0
				text=string:Mit Bestehen der Prüfungen kannst du die Vergangenheit dieses Landes erfahren, und du wirst wissen, wer dein Vater ist. Du wirst lernen, wer du selber bist: All das ist Vorraussetzung dafür, dass du Magier werden kannst.
				name=string:DIA_DIC_Lagariman_Lernen_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6622]
		blockName=string:DIA_DIC_Lagariman_Woelfe_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6623]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6624]
				subType=enum:0
				text=string:Wo finde ich die Wölfe?
				name=string:DIA_DIC_Lagariman_Woelfe_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6625]
		blockName=string:DIA_DIC_Lagariman_Woelfe_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6626]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6627]
				subType=enum:0
				text=string:Sie haben sich in einer Ansiedlung niedergelassen. Seitdem die Siedlung von Menschen verlassen wurde, wird sie von den Wölfen bewohnt.
				name=string:DIA_DIC_Lagariman_Woelfe_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6628]
		blockName=string:DIA_DIC_Lagariman_Ork_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6629]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6630]
				subType=enum:0
				text=string:Wo finde ich den Ork?
				name=string:DIA_DIC_Lagariman_Ork_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6631]
		blockName=string:DIA_DIC_Lagariman_Ork_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6632]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6633]
				subType=enum:0
				text=string:In der Nähe der Siedlung die jetzt von Wölfen bewohnt ist.
				name=string:DIA_DIC_Lagariman_Ork_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6634]
		blockName=string:DIA_DIC_Lagariman_Bestanden_07_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6635]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6636]
				subType=enum:0
				text=string:Du hast die Prüfung bestanden! Ich freue mich über dein Können.
				name=string:DIA_DIC_Lagariman_Bestanden_07_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6637]
		blockName=string:DIA_DIC_Lagariman_Bestanden_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6638]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6639]
				subType=enum:0
				text=string:Du hast sicherlich gemerkt das du stärker geworden bist.
				name=string:DIA_DIC_Lagariman_Bestanden_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6640]
		blockName=string:DIA_DIC_Lagariman_Bestanden_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6641]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6642]
				subType=enum:0
				text=string:Mit Bestehen jeder Prüfung, wird etwas von deiner magischen Kraft freigesetzt, die noch ungenutzt in dir liegt.
				name=string:DIA_DIC_Lagariman_Bestanden_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6643]
		blockName=string:DIA_DIC_Lagariman_Bestanden_15_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6644]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6645]
				subType=enum:0
				text=string:Welche Prüfung steht mir als nächstes bevor?
				name=string:DIA_DIC_Lagariman_Bestanden_15_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6646]
		blockName=string:DIA_DIC_Lagariman_Bestanden_07_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6647]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6648]
				subType=enum:0
				text=string:Es ist die Prüfung des Geistes. Du musst vorsichtig vorgehen. Bestehst du sie nicht, wirst du deinen Verstand verlieren. Auf deinem Weg wirst du auf Menschen treffen, die es zu prüfen gilt. Über jeden musst du ein Urteil fällen. Von manchen wirst du erkennen, dass sie dir nach dem Leben trachten.
				name=string:DIA_DIC_Lagariman_Bestanden_07_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6649]
		blockName=string:DIA_DIC_Lagariman_Bestanden_07_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6650]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6651]
				subType=enum:0
				text=string:Doch bevor du zur Tat schreitest, lass uns eine kleine Pause machen. (kramt) Wo ist es denn, hm...,das? Pfui, das fault ja schon seit Wochen vor sich hin... weg damit! ...Ahhh, hier, da ist es.
				name=string:DIA_DIC_Lagariman_Bestanden_07_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6652]
		blockName=string:DIA_DIC_Lagariman_Bestanden_07_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6653]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6654]
				subType=enum:0
				text=string:Völlig unversehrt... Sumpf-Helmkraut, sehr selten, sehr schwer zu kriegen, sehr köstlich! Lange Zeit habe ich es aufbewahrt, und für diesen Anlass ist es gerade gut genug. Hier! Nimm was von diesem erlesenen Tabak, und setz dich zu mir. Lass uns für einen Moment die Ruhe genießen.
				name=string:DIA_DIC_Lagariman_Bestanden_07_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6655]
		blockName=string:DIA_DIC_Lagariman_Geistes_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6656]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6657]
				subType=enum:0
				text=string:Du sprachst von der Prüfung des Geistes. Was habe ich zu tun?
				name=string:DIA_DIC_Lagariman_Geistes_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6658]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6659]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6660]
				subType=enum:0
				text=string:Ich werde dich gleich zu einem Berg führen. Der erste Teil deiner Aufgabe ist es, einen Eingang zu finden, der dir einen Weg ins Innere weist. In der Höhle wirst du nach einer magischen Rune suchen. Die Rune des Geistes.
				name=string:DIA_DIC_Lagariman_Geistes_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6661]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6662]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6663]
				subType=enum:0
				text=string:Aber zuvor höre dir die Vorgeschichte an: Die Höhle ist seit langer Zeit bewohnt, in ihr leben eine Gruppe von Magiern, die von deinem Kommen nichts ahnen. Diese Magier sind seit langer Zeit dort in der Höhle eingeschlossen.
				name=string:DIA_DIC_Lagariman_Geistes_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6664]
		blockName=string:DIA_DIC_Lagariman_Geistes_15_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6665]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6666]
				subType=enum:0
				text=string:Wer sind diese Magier?
				name=string:DIA_DIC_Lagariman_Geistes_15_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6667]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6668]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6669]
				subType=enum:0
				text=string:Sie gehören zum Orden der Madrimier. Ihre Stadt fiel einst dem gewaltigen Orkangriff zum Opfer, von dem ich dir bereits erzählte. An dem Tag der Schlacht flüchtete sich diese Gruppe der Magier in die Höhle.
				name=string:DIA_DIC_Lagariman_Geistes_07_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6670]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6671]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6672]
				subType=enum:0
				text=string:Doch später lies eine Explosion einen Steinschlag herabregnen und der Eingang wurde verschüttet. Und so gab es für die Magier kein Entkommen. Die Felswände waren zu stark, kein Zauber konnte diese Steinmassen zum Bersten bringen.
				name=string:DIA_DIC_Lagariman_Geistes_07_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6673]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6674]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6675]
				subType=enum:0
				text=string:Die Magier hofften darauf, dass ihre Brüder nach ihnen suchen und sie befreien würden. Doch niemand kam.
				name=string:DIA_DIC_Lagariman_Geistes_07_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6676]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6677]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6678]
				subType=enum:0
				text=string:Nach einigen Jahren nutzten sie ihre restlichen Kräfte um einen Zauber zu schaffen der ihr Leben verlängert.
				name=string:DIA_DIC_Lagariman_Geistes_07_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6679]
		blockName=string:DIA_DIC_Lagariman_Geistes_15_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6680]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6681]
				subType=enum:0
				text=string:Was geschah im Laufe der Zeit mit den Magiern?
				name=string:DIA_DIC_Lagariman_Geistes_15_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6682]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_09
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6683]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6684]
				subType=enum:0
				text=string:Erst lebten sie friedlich zusammen. Doch dann stellten sie fest, dass einige von ihnen geistig nicht mehr klar denken konnten. Teilweise verfielen sie dem Wahnsinn. Es kam soweit, dass einige der Magier sich gegenseitig töteten.
				name=string:DIA_DIC_Lagariman_Geistes_07_09.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6685]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_10
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6686]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6687]
				subType=enum:0
				text=string:Einer unter ihnen, Eustace zog sich zurück und erschuf eine Rune, die die Gesundheit des Verstandes wieder herstellen sollte. Die "Rune des Geistes". Als die Rune fertiggestellt war, wurde Eustace getötet.
				name=string:DIA_DIC_Lagariman_Geistes_07_10.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6688]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_11
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6689]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6690]
				subType=enum:0
				text=string:In der Höhle stellte sich dann der Magier Feordwin als Anführer über all jene die noch bei Sinnen waren. Soweit ich es jetzt beobachten konnte, geht er seit Jahren einigen Studien der schwarzen Magie nach.
				name=string:DIA_DIC_Lagariman_Geistes_07_11.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6691]
		blockName=string:DIA_DIC_Lagariman_Geistes_07_12
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6692]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6693]
				subType=enum:0
				text=string:Er ist aber selbst wahnsinnig und dem Bösen verfallen. Du musst ihm die "Rune des Geistes" abnehmen. Alle Gutgesinnten die du in der Höhle antriffst, solltest dann du zu mir führen.
				name=string:DIA_DIC_Lagariman_Geistes_07_12.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6694]
		blockName=string:DIA_DIC_Lagariman_Bringen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6695]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6696]
				subType=enum:0
				text=string:Warum soll ich sie zu dir bringen?
				name=string:DIA_DIC_Lagariman_Bringen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6697]
		blockName=string:DIA_DIC_Lagariman_Bringen_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6698]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6699]
				subType=enum:0
				text=string:Ich habe erfahren das meine Aufgabe darüber hinausgeht dich auszubilden, junger Ardaric. Mein Entschluss steht fest. Mein Ziel ist es Berengar zu töten! Der Anschlag auf den König darf niemals ausgeführt werden. Ich werde bald aufbrechen und einige Freunde aufsuchen.
				name=string:DIA_DIC_Lagariman_Bringen_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6700]
		blockName=string:DIA_DIC_Lagariman_Bringen_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6701]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6702]
				subType=enum:0
				text=string:Von ihnen erhoffe ich mir Mithilfe. Und selbst wenn ich keine Magier finde, die mir beistehen, so versuche ich, jeden kampffähigen Mann für mich zu gewinnen. Ich werde sie von der heiligen Aufgabe überzeugen, die jedes Menschen Pflicht ist.
				name=string:DIA_DIC_Lagariman_Bringen_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6703]
		blockName=string:DIA_DIC_Lagariman_Bringen_07_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6704]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6705]
				subType=enum:0
				text=string:Denn wer im Angesicht des Bösen nicht zur Waffe greift, den ereilt schon bald die dunkle Macht und  fortan wäre sein Leben nichts anderes mehr, als ein elender Schatten.
				name=string:DIA_DIC_Lagariman_Bringen_07_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6706]
		blockName=string:DIA_DIC_Lagariman_Bringen_07_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6707]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6708]
				subType=enum:0
				text=string:Wenn du die Magier zu mir bringst, so hoffe ich, dass einige von ihnen mir helfen werden und ich hoffe auch, dass du mir zur Seite stehen wirst. Doch ich möchte dich zu nichts zwingen, ich gebe dir Zeit, um deinen Entscheidung zu bedenken.
				name=string:DIA_DIC_Lagariman_Bringen_07_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6709]
		blockName=string:DIA_DIC_Lagariman_Weisst_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6710]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6711]
				subType=enum:0
				text=string:Woher weißt du das alles?
				name=string:DIA_DIC_Lagariman_Weisst_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6712]
		blockName=string:DIA_DIC_Lagariman_Weisst_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6713]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6714]
				subType=enum:0
				text=string:Über die Zauber die mich sehend machen, kann ich noch nicht sprechen. Erst später, wenn dein Geist würdig ist, kann ich darüber reden.
				name=string:DIA_DIC_Lagariman_Weisst_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6715]
		blockName=string:DIA_DIC_Lagariman_Draussen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6716]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6717]
				subType=enum:0
				text=string:Wenn es einen Eingang gibt, warum nutzten die Magier diesen Weg nicht, um nach draußen zu gelangen?
				name=string:DIA_DIC_Lagariman_Draussen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6718]
		blockName=string:DIA_DIC_Lagariman_Draussen_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6719]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6720]
