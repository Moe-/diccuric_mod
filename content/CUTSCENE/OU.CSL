	[% zCCSBlock 0 11356]
		blockName=string:DIA_DIC_Feordwin_Bedeuten_08_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11357]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11358]
				subType=enum:0
				text=string:Feordwin, deine Herrschaft ist zuende. Im Namen der Madrimier, schließe ich dich aus der Bruderschaft aus. Gib uns die Rune des Geistes und behellige uns nicht, wenn wir diesen Ort ewiger Dunkelheit verlassen.
				name=string:DIA_DIC_Feordwin_Bedeuten_08_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11359]
