		subBlock0=float:0
		[% zCCSAtomicBlock 0 8345]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8346]
				subType=enum:0
				text=string:Da hast du dir was vorgenommen. Die einzigen, die ins Waschhaus dürfen, sind die Wächter. Na ja, hochrangige Magier oder Krieger auch, allerdings werden die besseres zu tun haben, als dort hinzugehen.
				name=string:DIA_DIC_Alwin_Lynn_01_01.WAV
			[]
		[]
	[]
