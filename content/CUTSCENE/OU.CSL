		[% zCCSAtomicBlock 0 11147]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11148]
				subType=enum:0
				text=string:Aber ich muss mit ihm sprechen.
				name=string:DIA_DIC_2082_Waechter_Stop_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11149]
		blockName=string:DIA_DIC_2082_Waechter_Stop_06_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11150]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11151]
				subType=enum:0
				text=string:Wenn es was wichtiges gibt, richte es Graveyarn aus.
				name=string:DIA_DIC_2082_Waechter_Stop_06_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11152]
		blockName=string:DIA_DIC_Modares_Wachen_11_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11153]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11154]
				subType=enum:0
				text=string:Hast dich wohl an den Wachen vorbeigeschlichen. Nicht ungeschickt! Ha ha. Aber das ist das letzte was dir gelungen ist, denn du hast deinen Tod selbst herbeigeführt.
				name=string:DIA_DIC_Modares_Wachen_11_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11155]
		blockName=string:DIA_DIC_Feordwin_Bedeuten_09_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11156]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11157]
				subType=enum:0
				text=string:Was hat das zu bedeuten? Amalric! Antworte!
				name=string:DIA_DIC_Feordwin_Bedeuten_09_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11158]
		blockName=string:DIA_DIC_Feordwin_Bedeuten_09_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11159]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11160]
				subType=enum:0
				text=string:Du Irrgläubiger. Glaubst du ich begreife nicht was hier vorgeht? Der Spitzbube da an deiner Seite: Merkst du denn nicht, das du unter einem schlechten Einfluss stehst. Nicht gegen mich solltest du dich wenden, sondern gegen diesen kleinen Bastard!
				name=string:DIA_DIC_Feordwin_Bedeuten_09_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11161]
		blockName=string:DIA_DIC_Gamel_Freund_12_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11162]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11163]
				subType=enum:0
				text=string:Ich habe schon gehört, dass du mit Amalric gesprochen hast! Aber ich will mich zuerst mit dir unterhalten, bevor wir über andere Dinge reden. Und was gibt es schöneres, als über Leben und Tod zu sprechen. Worüber möchtest du mit mir reden?
				name=string:DIA_DIC_Gamel_Freund_12_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11164]
		blockName=string:DIA_DIC_Gamel_Leben_13_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11165]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11166]
				subType=enum:0
				text=string:Leben.
				name=string:DIA_DIC_Gamel_Leben_13_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11167]
		blockName=string:DIA_DIC_Gamel_Leben_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11168]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11169]
				subType=enum:0
				text=string:Das Leben ist, wie in ein Boot zu steigen, das aufs Meer hinausfährt und ewig weiterfährt. Was denkst du darüber?
				name=string:DIA_DIC_Gamel_Leben_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11170]
		blockName=string:DIA_DIC_Gamel_Weitergehen_13_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11171]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11172]
				subType=enum:0
				text=string:Ja, so sehe ich das auch. Es muss immer weitergehen!
				name=string:DIA_DIC_Gamel_Weitergehen_13_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11173]
		blockName=string:DIA_DIC_Gamel_Weitergehen_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11174]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11175]
				subType=enum:0
				text=string:Aha, dann geh du auch mal weiter und lass mich mit meinen Überlegungen alleine.
				name=string:DIA_DIC_Gamel_Weitergehen_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11176]
		blockName=string:DIA_DIC_Gamel_Unlogisch_13_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11177]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11178]
				subType=enum:0
				text=string:Nein, ich glaube das ist unlogisch. Das Leben geht ja nicht immer weiter. Zumindest wird es durch den Tod unterbrochen. Deswegen müsste der Vers doch heißen: Das Leben ist, wie in ein Boot zu steigen, das aufs Meer hinausfährt und sinkt.
				name=string:DIA_DIC_Gamel_Unlogisch_13_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11179]
		blockName=string:DIA_DIC_Gamel_Unlogisch_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11180]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11181]
				subType=enum:0
				text=string:Ein interessanter Gedanke, den du da hervorgebracht hast. Da ich schon weiß, was du mit Amalric besprochen hast, brauchst du mich gar nicht fragen. Ich werde euch beistehen. Aber ich benötige einen Zauber mit dem ich den Wind lenken kann. Ohne ihn werde ich im Kampf nichts ausrichten.
				name=string:DIA_DIC_Gamel_Unlogisch_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11182]
		blockName=string:DIA_DIC_Gamel_Tod_13_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11183]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11184]
				subType=enum:0
				text=string:Tod.
				name=string:DIA_DIC_Gamel_Tod_13_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11185]
		blockName=string:DIA_DIC_Gamel_Tod_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11186]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11187]
				subType=enum:0
				text=string:Natürlich gibt es den Tod. Und es gibt nach dem Tod keinen Übergang in eine andere Welt. Was meinst du dazu?
				name=string:DIA_DIC_Gamel_Tod_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11188]
		blockName=string:DIA_DIC_Gamel_Meinung_13_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11189]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11190]
				subType=enum:0
				text=string:Ja, ganz meine Meinung!
				name=string:DIA_DIC_Gamel_Meinung_13_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11191]
		blockName=string:DIA_DIC_Gamel_Meinung_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11192]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11193]
				subType=enum:0
				text=string:Hm. Gut, mit dir mal geredet zu haben. Bitte störe mich jetzt nicht weiter.
				name=string:DIA_DIC_Gamel_Meinung_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11194]
		blockName=string:DIA_DIC_Gamel_Anderes_13_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11195]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11196]
				subType=enum:0
				text=string:Nein. Ich könnte mir auch etwas anderes vorstellen: Durch den Tod, tritt etwas neues ein. Deswegen muss für den Menschen der stirbt nach dem Tod etwas dasein, was er noch nicht kannte, eine andere Welt.
				name=string:DIA_DIC_Gamel_Anderes_13_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11197]
		blockName=string:DIA_DIC_Gamel_Anderes_13_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11198]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11199]
				subType=enum:0
				text=string:Wie der Mensch diese Welt wahrnimmt, lässt sich nicht sagen. Wenn es aber eine andere Welt gibt, führt der Tod den Menschen dorthin. Daher würde ich deinen Gedanken so formulieren: Es gibt keinen Tod. Es gibt nur den Übergang in eine andere Welt.
				name=string:DIA_DIC_Gamel_Anderes_13_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11200]
		blockName=string:DIA_DIC_Gamel_Anderes_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11201]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11202]
				subType=enum:0
				text=string:Wahrhaftig, ein Gedanke, der die Sinne zu weiteren Überlegungen öffnet. Aber lass uns nicht weiter über unlösbare Rätsel der Welt sprechen, während dringlicheres ansteht und von uns Taten erfordert.
				name=string:DIA_DIC_Gamel_Anderes_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11203]
		blockName=string:DIA_DIC_Gamel_Anderes_15_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11204]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11205]
				subType=enum:0
				text=string:Ich habe schon gehört, was du mit Amalric besprochen hast. Und ich werde dir helfen. Aber ich benötige einen Zauber mit dem ich den Wind lenken kann. Ohne ihn werde ich im Kampf nichts ausrichten.
				name=string:DIA_DIC_Gamel_Anderes_15_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11206]
		blockName=string:DIA_DIC_Gamel_Geben_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11207]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11208]
				subType=enum:0
				text=string:Hier ist der Zauber, nach dem du fragst.
				name=string:DIA_DIC_Gamel_Geben_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11209]
		blockName=string:DIA_DIC_Gamel_Geben_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11210]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11211]
				subType=enum:0
				text=string:Du findest mich bei Amalric.
				name=string:DIA_DIC_Gamel_Geben_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11212]
		blockName=string:DIA_DIC_Herlewin_Freund_12_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11213]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11214]
				subType=enum:0
				text=string:Wenn ich nur wüsste, was Argantael damals tat. Damals im Kampf gegen die Grirawyr. Wann war es noch und was tat er damals? War es Argantael der eine entscheidende Rolle spielte, ich weiss es nicht mehr, verdammt, ich muss mich konzentrieren.
				name=string:DIA_DIC_Herlewin_Freund_12_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11215]
		blockName=string:DIA_DIC_Herlewin_Hier_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11216]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11217]
				subType=enum:0
				text=string:Herlewin, ich kann dir helfen!
				name=string:DIA_DIC_Herlewin_Hier_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11218]
		blockName=string:DIA_DIC_Herlewin_Hier_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11219]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11220]
				subType=enum:0
				text=string:Argantael war ein Zauberer der im Jahre 1342  in der Schlacht gegen die Grirawyr einen Nebel heraufbeschwor, der vom Wind nicht verweht werden konnte. In der Schlacht war der Nebel von entscheidendem Vorteil. Die Angreifer waren in der Unterzahl aber die feindlichen Grigrawyr glaubten, gegen ein mächtiges Herr anzutreten und flohen schließlich.
				name=string:DIA_DIC_Herlewin_Hier_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11221]
		blockName=string:DIA_DIC_Herlewin_Hier_12_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11222]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11223]
				subType=enum:0
				text=string:Ja! So ist es. Seit ein paar Tagen grübele ich vergeblich darüber, wie es sich mit dem wahrheitsgemässen Verlauf der Geschichte zutrug. Ein Buch in dem ich mein Wissen vor 18 Jahren niederschrieb, habe ich verloren. Danke für deine Hilfe. Aber sage mir, wer bist du?
				name=string:DIA_DIC_Herlewin_Hier_12_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11224]
		blockName=string:DIA_DIC_Herlewin_Hier_15_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11225]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11226]
				subType=enum:0
				text=string:Mein Name ist Ardaric.  Ich habe erst seit kurzem einen Weg in die Höhle gefunden. Amalric und ich haben uns dazu entschlossen, gegen Feordwin  anzutreten. Ich möchte dich um deine Hilfe im Kampf gegen Feordwin bitten.
				name=string:DIA_DIC_Herlewin_Hier_15_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11227]
		blockName=string:DIA_DIC_Herlewin_Hier_12_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11228]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11229]
				subType=enum:0
				text=string:Ich helfe dir. Aber ich brauche einen Kampf Zauber mit dem man Wasser gefrieren könnte. Erst dann kann ich euch wirklich helfen.
				name=string:DIA_DIC_Herlewin_Hier_12_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11230]
		blockName=string:DIA_DIC_Herlewin_Zauber_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11231]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11232]
				subType=enum:0
				text=string:Gut. Warte hier auf mich.
				name=string:DIA_DIC_Herlewin_Zauber_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11233]
		blockName=string:DIA_DIC_Herlewin_Geben_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11234]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11235]
				subType=enum:0
				text=string:Hier ist der Zauber, nach dem du fragst.
				name=string:DIA_DIC_Herlewin_Geben_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11236]
		blockName=string:DIA_DIC_Herlewin_Geben_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11237]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11238]
				subType=enum:0
				text=string:Du findest mich bei Amalric.
				name=string:DIA_DIC_Herlewin_Geben_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11239]
		blockName=string:DIA_DIC_Waleran_Freund_11_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11240]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11241]
				subType=enum:0
				text=string:Ich suche einen Satz der in ein Rätsel passt! Wie würdest du den Begriff der Zeit umschreiben?
				name=string:DIA_DIC_Waleran_Freund_11_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11242]
		blockName=string:DIA_DIC_Waleran_Verschlingt_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11243]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11244]
				subType=enum:0
				text=string:Etwas, das alles und jeden verschlingt: Zerbeißt jedes Schwert, zermalmt den härtesten Stein.
				name=string:DIA_DIC_Waleran_Verschlingt_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11245]
		blockName=string:DIA_DIC_Waleran_Verschlingt_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11246]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11247]
				subType=enum:0
				text=string:Ja, das ist eine gute Lösung.
				name=string:DIA_DIC_Waleran_Verschlingt_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11248]
		blockName=string:DIA_DIC_Waleran_Verschlingt_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11249]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11250]
				subType=enum:0
				text=string:Willst du mir helfen gegen Feordwin zu kämpfen. Amalric hat mir seine Hilfe schon angeboten. Doch wir brauchen noch Magier die uns unterstützen.
				name=string:DIA_DIC_Waleran_Verschlingt_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11251]
		blockName=string:DIA_DIC_Waleran_Verschlingt_11_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11252]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11253]
				subType=enum:0
				text=string:Gut, ich helfe dir. Doch damit ich für unsere Freiheit antreten kann brauchen ich einen Zauber der mir das Feuer zum Untertan macht.
				name=string:DIA_DIC_Waleran_Verschlingt_11_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11254]
		blockName=string:DIA_DIC_Waleran_Halten_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11255]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11256]
				subType=enum:0
				text=string:Man kann es halten, man kann es aber nicht kaufen.
				name=string:DIA_DIC_Waleran_Halten_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11257]
		blockName=string:DIA_DIC_Waleran_Halten_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11258]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11259]
				subType=enum:0
				text=string:So so, bitte störe mich nicht weiter.
				name=string:DIA_DIC_Waleran_Halten_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11260]
		blockName=string:DIA_DIC_Waleran_Eilt_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11261]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11262]
				subType=enum:0
				text=string:Es eilt nicht, es kann nur stillstehen und ruhen.
				name=string:DIA_DIC_Waleran_Eilt_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11263]
		blockName=string:DIA_DIC_Waleran_Eilt_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11264]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11265]
				subType=enum:0
				text=string:Aha. Entschuldige mich, ich habe zu tun.
				name=string:DIA_DIC_Waleran_Eilt_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11266]
		blockName=string:DIA_DIC_Waleran_Zauber_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11267]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11268]
				subType=enum:0
				text=string:Warte. Ich werde dir den Zauber besorgen.
				name=string:DIA_DIC_Waleran_Zauber_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11269]
		blockName=string:DIA_DIC_Waleran_Geben_15_00
		numOfBlocks=int:1
