		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8348]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8349]
				subType=enum:0
				text=string:Vielleicht hast du ja Glück, und es interessiert niemanden, was du im Waschhaus willst. Aber wie du eine Frau von dort zur Burg bringen willst... das weiß ich nicht
				name=string:DIA_DIC_Alwin_Lynn_01_02.WAV
			[]
		[]
