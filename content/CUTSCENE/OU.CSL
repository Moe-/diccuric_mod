	[% zCCSBlock 0 8665]
		blockName=string:DIA_DIC_Alwin_KAufBaltar_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8666]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8667]
				subType=enum:0
				text=string:Obwohl ich jetzt im Dienst Berengars stehe, habe ich nichts zu tun. Berengar selbst hatte nur wenig Zeit mit mir zu reden.
