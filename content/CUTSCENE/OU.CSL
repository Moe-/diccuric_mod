		subBlock0=float:0
		[% zCCSAtomicBlock 0 8708]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8709]
				subType=enum:0
				text=string:Berengar schätzt es wenn jemand der Sprache Herr ist. Für ihn ist das ein Zeichen eines klaren Verstandes.
				name=string:DIA_dic_Alwin_GraveRhet_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8710]
		blockName=string:DIA_dic_Alwin_GraveRhet_01_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8711]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8712]
				subType=enum:0
				text=string:Und er achtet jene die einen klaren Verstand haben und die nicht nur auf Muskelkraft setzen, wenn sie im Kampf ihr Schwert führen.
				name=string:DIA_dic_Alwin_GraveRhet_01_02.WAV
			[]
