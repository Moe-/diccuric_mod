		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10247]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10248]
				subType=enum:0
				text=string:Die anderen sind im Einsatz. Wir haben unseren Trupp schon aufgeteilt. Zwei von uns sollten das Wolfsrudel umgehen und ihnen von hinten zusetzen. Doch irgendetwas muss ihnen zugestoßen sein.
				name=string:DIA_DIC_Narrow_Info_07_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10249]
		blockName=string:DIA_DIC_Narrow_Info_15_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10250]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10251]
