	[% zCCSBlock 0 5920]
		blockName=string:DIA_DIC_Monolog_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5921]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5922]
				subType=enum:0
				text=string:Wie lange sind wir jetzt hier im Lager? Zwei Monate, drei? Berengar habe ich in der ganzen Zeit nicht zu Gesicht bekommen.
				name=string:DIA_DIC_Monolog_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5923]
		blockName=string:DIA_DIC_Monolog_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5924]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5925]
				subType=enum:0
				text=string:Als ich ihm damals gefolgt bin, hätt´ ich nie im Leben geglaubt, dass wir so weit reisen werden.
				name=string:DIA_DIC_Monolog_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5926]
		blockName=string:DIA_DIC_Monolog_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5927]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5928]
				subType=enum:0
				text=string:Nun ja, fast sind wir wieder zurück in Gatyah. Obwohl, hier in der Festung Parthalan werden wir noch eine Weile bleiben.
				name=string:DIA_DIC_Monolog_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5929]
		blockName=string:DIA_DIC_Monolog_15_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5930]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5931]
				subType=enum:0
				text=string:Für mich gibt es viel zu tun. Ich bin es leid, dass die Söldner mich wie einen schäbigen Laufburschen behandeln. Ich muss endlich im Lager ernst genommen werden.
				name=string:DIA_DIC_Monolog_15_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5932]
		blockName=string:DIA_DIC_Monolog_15_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5933]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5934]
				subType=enum:0
				text=string:Und ich weiß, dass ich ein Krieger sein kann. Außerdem will ich ein Kämpfer an Berengars Seite werden.
				name=string:DIA_DIC_Monolog_15_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5935]
		blockName=string:DIA_DIC_Monolog_15_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5936]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5937]
				subType=enum:0
				text=string:Spätestens dann sieht niemand mehr einen Dienstboten in mir. Berengar wird seinen verlorenen Ruhm zurückerkämpfen, und ich werde seine rechte Hand dabei sein.
				name=string:DIA_DIC_Monolog_15_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5938]
		blockName=string:DIA_DIC_Monolog_15_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5939]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5940]
				subType=enum:0
				text=string:Wo ist eigentlich Alwin? Neben Berengar ist er der einzige, dem ich hier im Lager vertrauen kann. Egal...
				name=string:DIA_DIC_Monolog_15_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5941]
		blockName=string:DIA_DIC_Monolog_15_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5942]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5943]
				subType=enum:0
				text=string:Zuerst helfe ich den Söldnern hier. Berengar sagte, er hat ein Auge auf mich geworfen, und er riet mir, im Lager zu helfen, wo immer ich kann.
				name=string:DIA_DIC_Monolog_15_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5944]
		blockName=string:DIA_DIC_Monolog_15_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5945]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5946]
				subType=enum:0
				text=string:Ich weiß noch genau, was er zu mir sagte: "Der Tag wird kommen, an dem ich dich zu mir rufe.
				name=string:DIA_DIC_Monolog_15_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5947]
		blockName=string:DIA_DIC_Monolog_15_09
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5948]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5949]
				subType=enum:0
				text=string:Wenn sich zeigt, dass du würdig bist in meine Dienste zu treten, kannst du mit deiner Ausbildung zum Krieger beginnen." Ja, und wenn dieser Tag kommt, werde ich bereit sein.
				name=string:DIA_DIC_Monolog_15_09.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5950]
		blockName=string:TurmGefangen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5951]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5952]
				subType=enum:0
				text=string:So ein Mist. Da unten ist ein ganzes Wolfsrudel. Wenigstens hab ich ein wenig Fleisch zum Essen dabei und muss so nicht verhungern...aber ich sollte versuchen, in die Burg zurückzukommen.
				name=string:TurmGefangen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5953]
		blockName=string:FleischEssen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5954]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5955]
				subType=enum:0
				text=string:Vielleicht sollte ich aufhören, das Fleisch zu essen und mir damit was besseres anfangen?
				name=string:FleischEssen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5956]
		blockName=string:DIA_DIC_Schneider_Robe_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5957]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5958]
				subType=enum:0
				text=string:Hallo.
				name=string:DIA_DIC_Schneider_Robe_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5959]
		blockName=string:DIA_DIC_Schneider_Robe_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5960]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5961]
				subType=enum:0
				text=string:Was willst du? Es ist noch nichts fertig. Du kannst also noch nichts abholen.
				name=string:DIA_DIC_Schneider_Robe_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5962]
		blockName=string:DIA_DIC_Schneider_Kaufen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5963]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5964]
				subType=enum:0
				text=string:Ich will nichts abholen. Ich will etwas von dir kaufen.
				name=string:DIA_DIC_Schneider_Kaufen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5965]
		blockName=string:DIA_DIC_Schneider_Kaufen_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5966]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5967]
				subType=enum:0
				text=string:Du willst etwas kaufen? Du hast doch gar kein Silber. Für das, was du besitzt, kann ich dir höchstens ein paar Stofffetzen heraussuchen.
				name=string:DIA_DIC_Schneider_Kaufen_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5968]
		blockName=string:DIA_DIC_Schneider_Arbeit_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5969]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5970]
				subType=enum:0
				text=string:Ich bewundere deine kunstvolle Arbeit. Was für Gewänder fertigst du an?
				name=string:DIA_DIC_Schneider_Arbeit_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5971]
		blockName=string:DIA_DIC_Schneider_Arbeit_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5972]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5973]
				subType=enum:0
				text=string:Ich schneidere und nähe die Roben für die Magier.
				name=string:DIA_DIC_Schneider_Arbeit_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5974]
		blockName=string:DIA_DIC_Schneider_Arbeit_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5975]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5976]
				subType=enum:0
				text=string:Kannst du mir eine verkaufen?
				name=string:DIA_DIC_Schneider_Arbeit_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5977]
		blockName=string:DIA_DIC_Schneider_Arbeit_11_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5978]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5979]
				subType=enum:0
				text=string:Wenn du ein Magier bist, bekommst du sie ohne ein einziges Silber zu zahlen. Kaufen kann man sie nicht. Sonst könnte ja jeder mit einem Magier verwechselt werden, und hier überall rein und rausspazieren.
				name=string:DIA_DIC_Schneider_Arbeit_11_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5980]
		blockName=string:DIA_DIC_Schneider_Taliasan_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5981]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5982]
				subType=enum:0
				text=string:Ich werde bald ein Lehrling Taliasans. Eine Robe ist doch ein passendes Geschenk. Dadurch wird mir Taliasan schon vom ersten Tag an wohlgesonnen sein.
				name=string:DIA_DIC_Schneider_Taliasan_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5983]
		blockName=string:DIA_DIC_Schneider_Taliasan_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5984]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5985]
				subType=enum:0
				text=string:Wozu braucht Taliasan eine Robe? Was Berengar für ihn zu tun hat, kann er auch im Schlafrock erledigen, das heißt, wenn ihm Berengar etwas zu tun gäbe. Aber Handel ist Handel, und da Taliasan kein anerkannter Magier ist, muss er zahlen. Bring mir hundert Silbermünzen und du kriegst deine Robe.
				name=string:DIA_DIC_Schneider_Taliasan_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5986]
		blockName=string:DIA_DIC_Schneider_Taliasan_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5987]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5988]
				subType=enum:0
				text=string:Ok.
				name=string:DIA_DIC_Schneider_Taliasan_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5989]
		blockName=string:DIA_DIC_Schneider_Erz_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5990]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5991]
				subType=enum:0
				text=string:Hier ist das Silber.
				name=string:DIA_DIC_Schneider_Erz_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5992]
		blockName=string:DIA_DIC_Schneider_Erz_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5993]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5994]
				subType=enum:0
				text=string:Wenn du das Silber von Taliasan hast, sag mir nicht, wo er es aufgetrieben hat. Ich will in nichts reingezogen werden. Hier, die Robe.
				name=string:DIA_DIC_Schneider_Erz_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5995]
		blockName=string:DIA_DIC_Scheider_Erz_11_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5996]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 5997]
				subType=enum:0
				text=string:Du hast wohl noch nicht mit Taliasan über das Silber geredet...
				name=string:DIA_DIC_Scheider_Erz_11_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 5998]
		blockName=string:DIA_DIC_Taylor_Pyt_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 5999]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6000]
				subType=enum:0
				text=string:Hey. Du stehst doch öfter mit Pyt zusammen rum oder?
				name=string:DIA_DIC_Taylor_Pyt_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6001]
		blockName=string:DIA_DIC_Taylor_Pyt_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6002]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6003]
				subType=enum:0
				text=string:Und?
				name=string:DIA_DIC_Taylor_Pyt_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6004]
		blockName=string:DIA_DIC_Taylor_Pyt_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6005]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6006]
				subType=enum:0
				text=string:Ihr seit befreundet, stimmt's?
				name=string:DIA_DIC_Taylor_Pyt_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6007]
		blockName=string:DIA_DIC_Taylor_Pyt_07_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6008]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6009]
				subType=enum:0
				text=string:Ja. Und?
				name=string:DIA_DIC_Taylor_Pyt_07_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6010]
		blockName=string:DIA_DIC_Taylor_Pyt_15_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6011]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6012]
				subType=enum:0
				text=string:Ich habe gerade versucht, mit Pyt zu reden. Aber mehr als Beleidigungen bekam ich von ihm nicht zu hören. Er hat wohl einen schlechten Tag?
				name=string:DIA_DIC_Taylor_Pyt_15_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6013]
		blockName=string:DIA_DIC_Taylor_Pyt_07_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6014]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6015]
				subType=enum:0
				text=string:Pyt hat zurzeit nur schlechte Tage.
				name=string:DIA_DIC_Taylor_Pyt_07_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6016]
		blockName=string:DIA_DIC_Taylor_Pyt_15_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6017]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6018]
				subType=enum:0
				text=string:Warum?
				name=string:DIA_DIC_Taylor_Pyt_15_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6019]
		blockName=string:DIA_DIC_Taylor_Pyt_07_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6020]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6021]
				subType=enum:0
				text=string:Vor kurzem hat man ihn nachts an der Schildmauer überfallen. Überlebt hat er nur, weil ihn der Dieb für tot hielt. Nach dem Überfall lag Pyt Stunden vor der Burg, bewusstlos und schwerverletzt.
				name=string:DIA_DIC_Taylor_Pyt_07_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6022]
		blockName=string:DIA_DIC_Taylor_Pyt_07_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6023]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6024]
				subType=enum:0
				text=string:Sein gesamtes Silber hat der Dieb ihm genommen. Aber was für Pyt schwerer wiegt: Sein Schwert "Glücksbringer" ist auch gestohlen.
				name=string:DIA_DIC_Taylor_Pyt_07_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6025]
		blockName=string:DIA_DIC_Taylor_Pyt_15_09
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6026]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6027]
				subType=enum:0
				text=string:Ich wusste gar nicht, dass durch dieses Land Räuber ziehen.
				name=string:DIA_DIC_Taylor_Pyt_15_09.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6028]
		blockName=string:DIA_DIC_Taylor_Pyt_07_10
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6029]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6030]
				subType=enum:0
				text=string:Es sind auch keine Räuber. Hierher trauen sich nicht viele. Es ist einer von den Söldnern gewesen, Rotrou.
				name=string:DIA_DIC_Taylor_Pyt_07_10.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6031]
		blockName=string:DIA_DIC_Taylor_Ort_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6032]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6033]
				subType=enum:0
				text=string:Was weißt du über diesen Ort?
				name=string:DIA_DIC_Taylor_Ort_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6034]
		blockName=string:DIA_DIC_Taylor_Ort_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6035]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6036]
				subType=enum:0
				text=string:Nichts. Wie alle hier.
				name=string:DIA_DIC_Taylor_Ort_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6037]
		blockName=string:DIA_DIC_Taylor_Tun_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6038]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6039]
				subType=enum:0
				text=string:Was soll ich tun?
				name=string:DIA_DIC_Taylor_Tun_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6040]
		blockName=string:DIA_DIC_Taylor_Tun_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6041]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6042]
				subType=enum:0
				text=string:Was du tun sollst? Diese Geschichte nicht noch weiter rumerzählen. Obwohl, es weiß ja eh schon fast jeder.
				name=string:DIA_DIC_Taylor_Tun_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6043]
		blockName=string:DIA_DIC_Taylor_Wer_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6044]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6045]
				subType=enum:0
				text=string:Woher weißt du, dass es Rotrou war?
				name=string:DIA_DIC_Taylor_Wer_15_00.WAV
			[]
		[]
	[]
