		[]
	[]
	[% zCCSBlock 0 8386]
		blockName=string:DIA_DIC_TDetails_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8387]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8388]
				subType=enum:0
				text=string:Was weißt du noch über Taliasan?
				name=string:DIA_DIC_TDetails_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8389]
		blockName=string:DIA_DIC_TDetails_01_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8390]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8391]
				subType=enum:0
				text=string:Hab ich dir das nicht schon erzählt? Er soll dunkle Geschäfte betreiben. Er verändert die Wirkung von Kräuterpflanzen. Diese verkauft er, oder kocht aus ihnen einen Sud, den er in Flaschen abfüllt. Wenn sie nicht giftig wirken, führen seine Mittel zu starken Rauschzuständen! Es gibt hier irgendwo eine Gruppe von Leuten, die ihm seine Ware abnehmen. Aber Taliasan bietet nicht immer was an, das macht er nach Lust und Laune.
				name=string:DIA_DIC_TDetails_01_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8392]
		blockName=string:DIA_DIC_SDetails_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8393]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8394]
				subType=enum:0
				text=string:Was weißt du über Slade?
				name=string:DIA_DIC_SDetails_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8395]
		blockName=string:DIA_DIC_SDetails_01_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8396]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8397]
				subType=enum:0
				text=string:Er ist ein Halunke und Gauner. Nur für Berengar spart er sich seine Treue auf. Mit allen anderen führt er höchstens noch Geschäfte.
				name=string:DIA_DIC_SDetails_01_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8398]
		blockName=string:DIA_DIC_Rhetorik_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8399]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8400]
				subType=enum:0
				text=string:Sag mal Alwin, kannst du mir helfen, meine Redegewandtheit zu verbessern?
