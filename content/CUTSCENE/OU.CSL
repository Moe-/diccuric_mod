		[]
	[]
	[% zCCSBlock 0 10036]
		blockName=string:DIA_DIC_Gabb_Erwischt_09_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10037]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10038]
				subType=enum:0
				text=string:Graveyarn schickt mich. Ich denke mal, du wirst dir jetzt die Zeit nehmen, oder?
				name=string:DIA_DIC_Gabb_Erwischt_09_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10039]
		blockName=string:DIA_DIC_Gabb_Erwischt_15_09
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10040]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10041]
				subType=enum:0
				text=string:Und was?
				name=string:DIA_DIC_Gabb_Erwischt_15_09.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10042]
		blockName=string:DIA_DIC_Gabb_Erwischt_09_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10043]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10044]
				subType=enum:0
				text=string:Was hast du so an Sachen bei dir? Was ist das? Ein Brief? "Taliasans hervorragende Fähigkeiten. Von Pyt für Berengar" - Oh. Ich glaube nicht, dass Maldive darüber erfreut sein wird. Und erst Graveyarn wird nicht gerade glücklich sein, wenn er hört, dass du für Taliasan arbeitest. Denn darum geht es hier doch!
				name=string:DIA_DIC_Gabb_Erwischt_09_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10045]
		blockName=string:DIA_DIC_Gabb_Erwischt_15_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10046]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10047]
				subType=enum:0
				text=string:Ich arbeite nicht für ihn, nein. Der Brief ist ... für...
				name=string:DIA_DIC_Gabb_Erwischt_15_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10048]
		blockName=string:DIA_DIC_Gabb_Erwischt_09_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10049]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10050]
				subType=enum:0
				text=string:(lacht) Mach's gut Junge.
				name=string:DIA_DIC_Gabb_Erwischt_09_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10051]
		blockName=string:DIA_DIC_Gabb_Erwischt_15_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10052]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10053]
				subType=enum:0
				text=string:Warte... was soll ich tun, damit du nichts verrätst und mir den Brief zurück gibst?
				name=string:DIA_DIC_Gabb_Erwischt_15_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10054]
		blockName=string:DIA_DIC_Gabb_Erwischt_09_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10055]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10056]
				subType=enum:0
				text=string:Ha ha ha. Ok. Da wäre etwas. Du könntest etwas für mich erledigen.
				name=string:DIA_DIC_Gabb_Erwischt_09_08.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10057]
		blockName=string:DIA_DIC_Gabb_Erwischt_09_10
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10058]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10059]
				subType=enum:0
				text=string:In den Wäldern, westlich von hier, liegt die Leiche eines Spähers. Er sollte als Vorhut für unsere Jäger den Bestand der Wölfe zählen. Er war auf dem Rückweg, als die Wölfe ihn töteten. Ich brauche sein Notizbuch, denn Berengar braucht es. Nur bis jetzt hat sich noch keiner in den Wald hineingewagt, um das Buch zu holen.
				name=string:DIA_DIC_Gabb_Erwischt_09_10.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10060]
		blockName=string:DIA_DIC_Gabb_Erwischt_15_11
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10061]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10062]
				subType=enum:0
				text=string:Wieso schickt Berengar nicht seine Jäger in den Wald?
				name=string:DIA_DIC_Gabb_Erwischt_15_11.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10063]
		blockName=string:DIA_DIC_Gabb_Erwischt_09_12
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10064]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10065]
				subType=enum:0
				text=string:Berengar wird erst in ein oder zwei Tagen einen Trupp rausschicken. Aber wenn er hört, dass jemand so mutig war, das Buch vorher zu holen, dann wäre das sicherlich mit einer  Belohnung verbunden. Denn wenn Berengar einen Namen in guter Erinnerung behält, ist das mit Gold kaum zu bezahlen.
				name=string:DIA_DIC_Gabb_Erwischt_09_12.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10066]
		blockName=string:DIA_DIC_Gabb_Erwischt_15_13
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10067]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10068]
				subType=enum:0
				text=string:Und ich soll das Buch für dich da rausholen?
				name=string:DIA_DIC_Gabb_Erwischt_15_13.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10069]
		blockName=string:DIA_DIC_Gabb_Erwischt_09_14
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10070]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10071]
				subType=enum:0
				text=string:Nein. Du sollst nicht, du musst das Notizbuch da rausholen! Sonst landet dein Brief bei Graveyarn.
				name=string:DIA_DIC_Gabb_Erwischt_09_14.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10072]
		blockName=string:DIA_DIC_Gabb_Erwischt_15_15
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10073]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10074]
				subType=enum:0
				text=string:Mir bleibt wohl keine andere Wahl. Ich mach es.
				name=string:DIA_DIC_Gabb_Erwischt_15_15.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10075]
		blockName=string:DIA_DIC_Gabb_Block_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10076]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10077]
