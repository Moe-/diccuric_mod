		[]
	[]
	[% zCCSBlock 0 8359]
		blockName=string:DIA_DIC_Alwin_Lurker_01_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8360]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8361]
				subType=enum:0
				text=string:Aber ja! Nur innerhalb von vier Jahren brüten sie. Du solltest in dieser Zeit einem Lurker lieber nicht zu nahe kommen. Sie pflegen ihre Brut scharf zu bewachen und verteidigen sie bis aufs Blut.
				name=string:DIA_DIC_Alwin_Lurker_01_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8362]
		blockName=string:DIA_DIC_Alwin_Lurker_15_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8363]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8364]
				subType=enum:0
				text=string:Werd ich mir merken.
				name=string:DIA_DIC_Alwin_Lurker_15_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8365]
		blockName=string:DIA_DIC_Alwin_Lurker_01_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8366]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8367]
				subType=enum:0
				text=string:Ich hab mal die Geschichte gehört, dass ein Vogel sich ein Lurkerei geschnappt hat. Zweitausend Fuß vom Brutplatz entfernt, hat die Lurkermutter den Vogel ausfindig gemacht, und sich das noch unversehrte Ei aus dem Nest geholt. Einen außerordentlichen Geruchssinn haben sie. Und von dem diebischen Vogel sind nur noch ein paar Federn übriggeblieben.
				name=string:DIA_DIC_Alwin_Lurker_01_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8368]
		blockName=string:DIA_DIC_Alwin_Lurker_Woher_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8369]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8370]
				subType=enum:0
				text=string:Woher weißt du das?
				name=string:DIA_DIC_Alwin_Lurker_Woher_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8371]
