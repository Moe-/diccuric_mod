	[]
	[% zCCSBlock 0 10111]
		blockName=string:DIA_DIC_Slade_Buch_15_08
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10112]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10113]
				subType=enum:0
				text=string:Es ist so. Ich brauche einen Brief von dir, dringend. Der Brief soll an Berengar gerichtet sein. In dem Brief soll das magische Können Taliasans gerühmt werden.
