		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8339]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8340]
				subType=enum:0
				text=string:Ich kann nicht. Ich bin in erster Linie Graveyarns Aufträgen verpflichtet.
				name=string:DIA_DIC_Alwin_Vater_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8341]
