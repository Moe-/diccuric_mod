	[% zCCSBlock 0 8374]
		blockName=string:DIA_DIC_Alwin_Lurker_Baeume_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8375]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8376]
				subType=enum:0
				text=string:Ich wusste gar nicht, dass Lurker Bäume hochklettern können.
				name=string:DIA_DIC_Alwin_Lurker_Baeume_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8377]
		blockName=string:DIA_DIC_Alwin_Lurker_Baeume_01_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8378]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8379]
				subType=enum:0
				text=string:Wenn sie ihr Junges verteidigen, würd' es mich auch nicht wundern, wenn sie an senkrechten Felswänden hochklettern.
				name=string:DIA_DIC_Alwin_Lurker_Baeume_01_01.WAV
