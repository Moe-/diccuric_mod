		subBlock0=float:0
		[% zCCSAtomicBlock 0 10118]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10119]
				subType=enum:0
				text=string:Ohne diesen Brief kann ich Graveyarns Prüfungen nicht ablegen, und wenn ich die Prüfungen nicht bestehe, wird mir jeder hier im Lager das Leben zur Hölle machen.
				name=string:DIA_DIC_Slade_Buch_15_10.WAV
