		blockName=string:DIA_DIC_Fisk_Haendler_12_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10742]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10743]
