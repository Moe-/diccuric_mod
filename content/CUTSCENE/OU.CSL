		subBlock0=float:0
		[% zCCSAtomicBlock 0 8423]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8424]
				subType=enum:0
				text=string:Unterweist du mich nun in die Grundregeln der Rhetorik?
				name=string:DIA_DIC_Alwin_Lernen_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8425]
		blockName=string:DIA_DIC_Alwin_Lernen_01_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8426]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8427]
				subType=enum:0
				text=string:Ja. Doch Graveyarn hat mir gerade einen Auftrag erteilt. Wenn du mir 100 Silber besorgst, kann ich jemand anderen damit "überreden", die Arbeit an meiner Stelle zu tun. Dann hätte ich Zeit für dich.
				name=string:DIA_DIC_Alwin_Lernen_01_03.WAV
