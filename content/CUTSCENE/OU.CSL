		[]
	[]
	[% zCCSBlock 0 11431]
		blockName=string:DIA_DIC_Malachias_Tust_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11432]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11433]
				subType=enum:0
				text=string:Sag mir was du hier tust, sonst rufe ich die Wachen.
				name=string:DIA_DIC_Malachias_Tust_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11434]
		blockName=string:DIA_DIC_Malachias_Tust_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11435]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11436]
				subType=enum:0
				text=string:Ruf die Wachen, junger Mann. Rufe sie. Ich bin zu alt, als das der Tod durch ein Schwert mich schreckt. Manchmal weiß ich nicht, ob ich lebe, oder schon tot bin. Sollen die Klingen der Söldner es mir sagen.
				name=string:DIA_DIC_Malachias_Tust_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11437]
		blockName=string:DIA_DIC_Malachias_Wachen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11438]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11439]
				subType=enum:0
				text=string:Ich rufe jetzt die Wachen!
				name=string:DIA_DIC_Malachias_Wachen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11440]
		blockName=string:DIA_DIC_Malachias_Wachen_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11441]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11442]
				subType=enum:0
				text=string:WACHEN!! Hier ist ein Eindringling!
				name=string:DIA_DIC_Malachias_Wachen_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11443]
		blockName=string:DIA_DIC_Malachias_Was_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11444]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11445]
				subType=enum:0
				text=string:Was tust du hier?
				name=string:DIA_DIC_Malachias_Was_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11446]
		blockName=string:DIA_DIC_Malachias_Was_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11447]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11448]
				subType=enum:0
				text=string:Ich bin ein Pilger. ...Ah, gibt es hier Steinbinsen-Kraut? Mein Rücken schmerzt. Als Sud gekocht hilft das Kraut gegen Schmerzen... ah, ah.  Was ich hier tue:  Ich bin auf dem Weg zum Heiligtum von Banachia. Es sind noch etwa achtzig Meilen nördlich bis dorthin. Meine Pilgerfahrt ist nach einem Kodex vorgeschrieben. Und mein Weg führt mich durch diese Burg.
				name=string:DIA_DIC_Malachias_Was_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11449]
		blockName=string:DIA_DIC_Malachias_Was_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11450]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11451]
				subType=enum:0
				text=string:Vor vielen Jahren war ich schon einmal hier: Damals war ich auf der Suche nach Reliquien zu Ehren Gottes. Einst war es hier sehr still. Die einzigen Lebwesen, die ich hier antraf, waren nistende Vögel zwischen den Turmzinnen. Dort, wo jetzt die Lanzen der Wächter hervorragen. Eine seltsame Welt ist es, die ihr Ungläubigen euch erbaut. Eine seltsame Welt.
				name=string:DIA_DIC_Malachias_Was_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11452]
		blockName=string:DIA_DIC_Malachias_Weg_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11453]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11454]
				subType=enum:0
				text=string:Dann noch viel Glück auf deinem Weg...
				name=string:DIA_DIC_Malachias_Weg_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11455]
		blockName=string:DIA_DIC_Malachias_Langen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11456]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11457]
				subType=enum:0
				text=string:Warum musst du den langen Weg auf dich nehmen?
				name=string:DIA_DIC_Malachias_Langen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11458]
		blockName=string:DIA_DIC_Malachias_LangenA_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11459]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11460]
				subType=enum:0
				text=string:Ich habe gegen eine Regel des Klosters verstoßen. Ich bin ein Sünder. Ich habe das Amt missbraucht, dass nur Auserwählten bestimmt ist. Ich habe Unglück heraufbeschworen, als wählte ich das Böse zum Freund.
				name=string:DIA_DIC_Malachias_LangenA_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11461]
		blockName=string:DIA_DIC_Malachias_LangenB_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11462]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11463]
				subType=enum:0
				text=string:Jetzt werde ich Buße tun: Nur wenn ich Banachia erreiche und ein Opfer für Gott erbringe, werde ich von meiner Sünde befreit. Doch bis dahin bin ich unwürdig, auch nur den Namen des Gottes zu erwähnen, dem ich diene.
				name=string:DIA_DIC_Malachias_LangenB_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11464]
		blockName=string:DIA_DIC_Malachias_Langen_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11465]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11466]
				subType=enum:0
				text=string:Was hast du für ein Verbrechen begangen?
				name=string:DIA_DIC_Malachias_Langen_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11467]
		blockName=string:DIA_DIC_Malachias_Langen_07_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11468]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11469]
				subType=enum:0
				text=string:Nach zwanzig Jahren ergebenen Dienstes, ernannte man mich zum Wächter des Himmels. In jeder großen Stadt in der unser Orden heimisch ist, gibt es dieses Amt. Meine Aufgabe bestand darin, von Beginn der Abenddämmerung bis zum Untergang der Sonne, auf dem Glockenturm des Klosters Wache zu halten. Doch nicht mit Lanzen, sondern mit den Augen und dem Verstand.
				name=string:DIA_DIC_Malachias_Langen_07_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11470]
		blockName=string:DIA_DIC_Malachias_Langen_07_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11471]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11472]
				subType=enum:0
				text=string:Nach einer alten Legende heißt es, dass ein Bote Gottes in Gestalt eines silbernen Vogels in der Zeit zwischen Dämmerung und Sonnenuntergang erscheinen wird. Er wird im Licht fliegen und dem Wächter des Himmels eine Botschaft Gottes überbringen. Und dies ist der Tag, an dem die Welt in göttliches Licht gehüllt sein wird. Und von da an, wird ewige Glückseligkeit herrschen.
				name=string:DIA_DIC_Malachias_Langen_07_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11473]
		blockName=string:DIA_DIC_Malachias_Langen_15_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11474]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11475]
				subType=enum:0
				text=string:Aber was war dein Vergehen?
				name=string:DIA_DIC_Malachias_Langen_15_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11476]
		blockName=string:DIA_DIC_Malachias_Langen_07_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11477]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11478]
				subType=enum:0
				text=string:Ich habe den Turm verlassen. Schande über mich. Schreie klangen von unten herauf, und ich erkannte in ihnen die Stimme einer Frau die mir Tage zuvor ihre Not schilderte. Etwas, worüber ich aufgrund eines Gelübdes nicht sprechen darf.
				name=string:DIA_DIC_Malachias_Langen_07_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11479]
		blockName=string:DIA_DIC_Malachias_Langen_07_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11480]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11481]
				subType=enum:0
				text=string:Ihr eilte ich zur Hilfe, denn sie wurde bis in die Kirche, in der sie Schutz suchte, verfolgt, und ihr Leben war bedroht. Doch ein Ordensbruder hatte gesehen, wie ich den Turm verließ. Zum Glück meldete er meine Schandtat dem Obersten des Ordens, und jetzt bin ich auf meiner Reise.
				name=string:DIA_DIC_Malachias_Langen_07_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11482]
		blockName=string:DIA_DIC_Malachias_Helfen_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11483]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11484]
				subType=enum:0
				text=string:Aber du wolltest doch jemandem helfen. Zählt das nicht?
				name=string:DIA_DIC_Malachias_Helfen_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11485]
		blockName=string:DIA_DIC_Malachias_Helfen_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11486]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11487]
				subType=enum:0
				text=string:Die Begrüßung eines göttlichen Boten ist bedeutsamer als ein Menschenleben. So jedenfalls haben die Weisen von jeher die Schriften ausgelegt. Und so ist es richtig. Stell dir vor was geschähe, wenn Gott gewahr wird, dass wir seiner Ankunft mit Nachlässigkeit begegnen.
				name=string:DIA_DIC_Malachias_Helfen_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11488]
		blockName=string:DIA_DIC_Malachias_Helfen_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11489]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11490]
				subType=enum:0
				text=string:Wenn wir zeigen würden, das wir nicht mit aller Hoffnung sein Kommen erbitten. Was glaubst du steht der Menschheit dann bevor?  Dann blüht uns niederen Geschöpfen eine Welt aus endlosem Unheil.
				name=string:DIA_DIC_Malachias_Helfen_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11491]
		blockName=string:DIA_DIC_Malachias_Reise_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11492]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11493]
				subType=enum:0
				text=string:Wenn deine Reise wirklich in Richtung Norden geht, dann hast du einen gefährlichen Weg vor dir. Ich habe gehört, dass die Sümpfe, die auf diesem Weg liegen, widrige Echsen beherbergen. Sie gewähren niemandem Einlass, es sei denn als Futter.
				name=string:DIA_DIC_Malachias_Reise_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11494]
		blockName=string:DIA_DIC_Malachias_Reise_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11495]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11496]
				subType=enum:0
				text=string:Gefährlich? Das stimmt. Im Laufe unserer Ordensgeschichte wanderten vierzehn Brüder nach Banachia. Keiner kam je dort an. Es heißt, Unwürdige erreichen Banachia nicht. Aber ich werde Banachia erreichen. Ich werde um Vergebung bitten.
				name=string:DIA_DIC_Malachias_Reise_07_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11497]
		blockName=string:Info_HighPriest1_17_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11498]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11499]
				subType=enum:0
				text=string:STEEERBLICHEEERRRR!!!!
				name=string:Info_HighPriest1_17_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11500]
		blockName=string:Info_HighPriest1_17_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11501]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11502]
				subType=enum:0
				text=string:DU WAAAGST ES, MEINE RUUHHE ZU STÖÖÖREN!
				name=string:Info_HighPriest1_17_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11503]
		blockName=string:Info_HighPriest1_17_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11504]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11505]
				subType=enum:0
				text=string:BEREEEITE DIIICH AUF DAS EEEENDE VOOOR!
				name=string:Info_HighPriest1_17_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11506]
		blockName=string:Info_Diego_Teach_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11507]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11508]
				subType=enum:0
				text=string:Ja. Bei mir kannst du lernen, deine Geschicklichkeit zu verbessern oder deine Kampfkraft zu steigern.
				name=string:Info_Diego_Teach_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11509]
		blockName=string:Info_Diego_Teach_BACK_11_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11510]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11511]
				subType=enum:0
				text=string:Deine Kampfkraft ist um einiges gestiegen.
				name=string:Info_Diego_Teach_BACK_11_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11512]
		blockName=string:Info_Diego_Teach_BACK_11_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11513]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11514]
				subType=enum:0
				text=string:Dein Geschick im Umgang mit Bögen und Armbrüsten ist schon besser geworden.
				name=string:Info_Diego_Teach_BACK_11_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 11515]
		blockName=string:Info_Diego_Teach_BACK_11_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11516]
