		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8408]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8409]
				subType=enum:0
				text=string:Das geht nicht. Fiebeler will zur Zeit nichts mit mir zu tun haben... das ist eine andere Geschichte.
				name=string:DIA_DIC_Rhetorik_01_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8410]
