	[% zCCSBlock 0 8380]
		blockName=string:DIA_DIC_Alwin_Slade_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8381]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8382]
				subType=enum:0
				text=string:Weißt du, wo ich Slade finde?
				name=string:DIA_DIC_Alwin_Slade_15_00.WAV
