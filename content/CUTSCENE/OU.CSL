		subBlock0=float:0
		[% zCCSAtomicBlock 0 8342]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8343]
				subType=enum:0
				text=string:Kannst du mir helfen? Ich soll für Raffa eine der Frauen aus den Waschhäusern zur Burg bringen.
				name=string:DIA_DIC_Alwin_Lynn_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8344]
