		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8447]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8448]
				subType=enum:0
				text=string:5.Ein überzeugender Schlusspunkt kann oftmals den Eindruck über die ganze Rede umkehren.
				name=string:DIA_DIC_Alwin_Erz_01_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8449]
		blockName=string:DIA_DIC_Alwin_Erz_15_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8450]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8451]
				subType=enum:0
				text=string:Auf was man alles beim Sprechen achten kann... Ich habe mir alles gemerkt. Vielleicht werde ich aufgrund meiner Aussprache mir jetzt leichter gehör verschaffen können. Danke für deine Hilfe.
				name=string:DIA_DIC_Alwin_Erz_15_07.WAV
