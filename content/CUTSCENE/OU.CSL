				name=string:DIA_DIC_Alwin_Erz_01_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8434]
		blockName=string:DIA_DIC_Alwin_Erz_01_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8435]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8436]
				subType=enum:0
				text=string:1. Sprich verständlich. Vereinfache Sachverhalte, die verschachtelt sind !
				name=string:DIA_DIC_Alwin_Erz_01_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8437]
		blockName=string:DIA_DIC_Alwin_Erz_01_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8438]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8439]
				subType=enum:0
				text=string:2. Bedenke, dass auch dein Körper spricht. Lass Mimik und Gestik sich natürlich entfalten.
				name=string:DIA_DIC_Alwin_Erz_01_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8440]
		blockName=string:DIA_DIC_Alwin_Erz_01_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8441]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8442]
				subType=enum:0
				text=string:3. Passe deine Rede deinem Gesprächspartner an. Bedenke, was der Zuhörer voraussetzt und erwartet.
				name=string:DIA_DIC_Alwin_Erz_01_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8443]
		blockName=string:DIA_DIC_Alwin_Erz_01_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8444]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8445]
				subType=enum:0
				text=string:4.Achte darauf, dass deine Argumente durchdacht sind und einen hohen Gehalt mit sich bringen.
				name=string:DIA_DIC_Alwin_Erz_01_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8446]
