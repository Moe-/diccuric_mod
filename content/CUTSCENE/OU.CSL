	[]
	[% zCCSBlock 0 11422]
		blockName=string:DIA_DIC_Malachias_Wer_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11423]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11424]
				subType=enum:0
				text=string:Oh, die Jugend! Da steht sie vor mir, und doch weiß sie nicht, wie weit entfernt sie von mir ist. Ich bin Malachias von Benthein, junger Mann.
