	[]
	[% zCCSBlock 0 11428]
		blockName=string:DIA_DIC_Malachias_Welchen_07_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 11429]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 11430]
				subType=enum:0
				text=string:Aufgaben? Ich unterstehe keinerlei Verpflichtungen, außer meinen eigenen.
