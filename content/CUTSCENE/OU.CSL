	[]
	[% zCCSBlock 0 6745]
		blockName=string:DIA_DIC_Lagariman_FuerDich_07_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 6746]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 6747]
				subType=enum:0
				text=string:Nach den Aufzeichnungen die ich über Etain studiert habe, gibt es hier mehrere verlassene Siedlungen. Versuche eine der früheren Madrimier Siedlungen zu finden. Je versteckter die Siedlung liegt, umso besser.
				name=string:DIA_DIC_Lagariman_FuerDich_07_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 6748]
