		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8315]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8316]
				subType=enum:0
				text=string:Wenn du ihn fragst, sei auf der Hut. Er ist sehr misstrauisch und wenn er jemanden nicht mag, kann er hinterhältig sein. Du wärst nicht der erste, der es erst merkt, wenn es zu spät ist!
				name=string:DIA_DIC_Alwin_THilfe_01_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8317]
		blockName=string:DIA_DIC_Alwin_Hinweis_01_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8318]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8319]
				subType=enum:0
				text=string:Ich muss mit dir sprechen. Einige Gerüchte sind hier im Umlauf. Nach dem, was ich gehört habe, hat einer von den Lagerarbeitern etwas über deinen Vater in Erfahrung gebracht.
				name=string:DIA_DIC_Alwin_Hinweis_01_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8320]
