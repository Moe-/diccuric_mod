	[% zCCSBlock 0 9994]
		blockName=string:DIA_DIC_Soeldner_Ei_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 9995]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 9996]
				subType=enum:0
				text=string:Hey. Was tust du hier?
				name=string:DIA_DIC_Soeldner_Ei_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 9997]
		blockName=string:DIA_DIC_Soeldner_Ei_14_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 9998]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 9999]
				subType=enum:0
				text=string:Wache halten. Wenn sich hier ein paar Fremde oder Orks herumtreiben, stehen Probleme an. Schickt Graveyarn dich?
				name=string:DIA_DIC_Soeldner_Ei_14_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10000]
		blockName=string:DIA_DIC_Soeldner_Pyt_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10001]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10002]
				subType=enum:0
				text=string:Nein. Mich schickt niemand. Aber ich habe gehört das du das Schwert von Pyt bei dir trägst. Du sollst es ihm geraubt haben!
				name=string:DIA_DIC_Soeldner_Pyt_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10003]
		blockName=string:DIA_DIC_Soeldner_Pyt_14_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10004]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10005]
				subType=enum:0
				text=string:Hat Pyt das gesagt? Natürlich! Wer sonst setzt solche hahnenbüchenen Geschichten in die Welt? Ich habe mir erst kürzlich ein Schwert neu anfertigen lassen. Dass Pyt jetzt behauptet es wäre seins, passt zu diesem Gauner!
				name=string:DIA_DIC_Soeldner_Pyt_14_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10006]
		blockName=string:DIA_DIC_Soeldner_Pyt_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10007]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10008]
				subType=enum:0
				text=string:Ah verstehe! Aber was sagst du dazu, wenn nicht nur Pyt sondern noch ein anderer darauf schwört, das du der Dieb bist? Ist das etwa Zufall?
				name=string:DIA_DIC_Soeldner_Pyt_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10009]
		blockName=string:DIA_DIC_Soeldner_Pyt_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10010]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10011]
				subType=enum:0
				text=string:Ja, genau, das ist Zufall.  Und wenn gleich meine Faust ausrutscht und ich dich niederschlage ist das auch Zufall.
				name=string:DIA_DIC_Soeldner_Pyt_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10012]
		blockName=string:DIA_DIC_Soeldner_Tausch_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10013]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10014]
				subType=enum:0
				text=string:Nein. Ich versuche mich gerade mit Tauschhandel über Wasser zuhalten. Vielleicht können wir etwas tauschen?
				name=string:DIA_DIC_Soeldner_Tausch_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10015]
		blockName=string:DIA_DIC_Soeldner_Tausch_14_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10016]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10017]
				subType=enum:0
				text=string:Warum nicht. Lass mal sehen, was du so dabei hast.
				name=string:DIA_DIC_Soeldner_Tausch_14_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10018]
		blockName=string:DIA_DIC_Soeldner_Tausch_14_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10019]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10020]
				subType=enum:0
				text=string:Oh ein Lurkerei. Das ist wahrhaft selten.
				name=string:DIA_DIC_Soeldner_Tausch_14_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10021]
		blockName=string:DIA_DIC_Soeldner_Tausch_15_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10022]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10023]
				subType=enum:0
				text=string:Bekomme ich dafür dein Schwert?
				name=string:DIA_DIC_Soeldner_Tausch_15_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10024]
		blockName=string:DIA_DIC_Soeldner_Tausch_15_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10025]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10026]
				subType=enum:0
				text=string:Ja klar. Dafür kann ich mir glatt drei neue Schwerter kaufen...
				name=string:DIA_DIC_Soeldner_Tausch_15_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10027]
		blockName=string:DIA_DIC_Soeldner_Tausch_14_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10028]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10029]
				subType=enum:0
				text=string:Du hast nichts brauchbares...
				name=string:DIA_DIC_Soeldner_Tausch_14_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10030]
		blockName=string:DIA_DIC_Gabb_Erwischt_09_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10031]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10032]
				subType=enum:0
				text=string:Hey Junge. Wohin des Wegs? Warte doch mal...
				name=string:DIA_DIC_Gabb_Erwischt_09_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 10033]
		blockName=string:DIA_DIC_Gabb_Erwischt_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 10034]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 10035]
				subType=enum:0
				text=string:Ich habe keine Zeit.
				name=string:DIA_DIC_Gabb_Erwischt_15_01.WAV
