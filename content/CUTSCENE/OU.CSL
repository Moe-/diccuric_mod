		blockName=string:DIA_DIC_Barem_BBesitzer_02_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7850]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7851]
				subType=enum:0
				text=string:Hm. Ich glaube dir nicht. Das Horn ist wertvoll und außerdem siehst du nicht aus wie ein Jäger. Also, wie bist du an das Horn gekommen?
				name=string:DIA_DIC_Barem_BBesitzer_02_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7852]
		blockName=string:DIA_DIC_Barem_BBesitzer_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7853]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7854]
				subType=enum:0
				text=string:Ich bin Jäger... doch, doch, ganz bestimmt. Das Horn habe ich ... gekauft, denke ich, ist schon lange her, äh.
				name=string:DIA_DIC_Barem_BBesitzer_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7855]
		blockName=string:DIA_DIC_Barem_BBesitzer_02_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7856]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7857]
				subType=enum:0
				text=string:Auch wenn ich glaube, dass du mich anschwindelst, hier hast du es. Ich brauche es nicht. Dass du mich angelogen hast, betrübt mich. Es gibt schon so, viele Betrüger auf dieser Welt.
				name=string:DIA_DIC_Barem_BBesitzer_02_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7858]
		blockName=string:DIA_DIC_Barem_Gewalt_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7859]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7860]
				subType=enum:0
				text=string:Mir werden hier zu viele Worte gewechselt! Gib mir das Horn oder stirb.
				name=string:DIA_DIC_Barem_Gewalt_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7861]
		blockName=string:DIA_DIC_Barem_Gekaempft_02_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7862]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7863]
				subType=enum:0
				text=string:Na? Ich wollte nicht kämpfen. Aber ich verzeihe dir deine ungestüme Jugend.
				name=string:DIA_DIC_Barem_Gekaempft_02_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7864]
		blockName=string:DIA_DIC_Barem_Gekaempft_15_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7865]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7866]
				subType=enum:0
				text=string:Das Horn gehört mir, und ich will es wiederhaben.
				name=string:DIA_DIC_Barem_Gekaempft_15_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7867]
		blockName=string:DIA_DIC_Barem_Gekaempft_02_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7868]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7869]
				subType=enum:0
				text=string:Auch wenn ich glaube, dass du mich anschwindelst, hier hast du es. Ich brauche es sowieso nicht. Dass du mich angelogen hast, betrübt mich. Es gibt schon so, viele Betrüger auf dieser Welt.
				name=string:DIA_DIC_Barem_Gekaempft_02_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7870]
		blockName=string:DIA_DIC_Baltar_Nachricht_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7871]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7872]
				subType=enum:0
				text=string:Ich habe eine Nachricht gefunden die mich beunruhigt. Hier steht, dass es eine Verschwörung gibt, mit dem Ziel euch zu ermorden.
				name=string:DIA_DIC_Baltar_Nachricht_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7873]
		blockName=string:DIA_DIC_Baltar_Nachricht_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7874]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7875]
				subType=enum:0
				text=string:Ich weiß davon. Ibba der einzige Mitverschwörer wird noch zur Rechenschaft gezogen. Noch steht er unter Beobachtung. Du solltest niemanden etwas darüber sagen.
				name=string:DIA_DIC_Baltar_Nachricht_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7876]
		blockName=string:DIA_DIC_Baltar_Nachricht_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7877]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7878]
				subType=enum:0
				text=string:Ich wusste von Anfang an, dass Gabb nicht nur Wein schmuggelt. Doch bevor ich dich ins Vertrauen ziehen konnte, musste ich erst wissen ob du es verdienst.
				name=string:DIA_DIC_Baltar_Nachricht_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7879]
		blockName=string:DIA_DIC_Baltar_AuftragA_05_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7880]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7881]
				subType=enum:0
				text=string:Es sind schon ein paar Monate vergangen, Ardaric, als wir uns das letzte Mal sahen. Ich hatte alle Hände voll zu tun, und das wird in Zukunft auch so bleiben. Trotz meiner Verpflichtungen habe ich mich bei Graveyarn immer über dein Wohlergehen erkundigt.
				name=string:DIA_DIC_Baltar_AuftragA_05_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7882]
		blockName=string:DIA_DIC_Baltar_AuftragA_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7883]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7884]
				subType=enum:0
				text=string:Du hattest es nicht immer leicht. Die ersten Prüfungen hast du bestanden, und damit gezeigt, wie ernst es dir ist in meine Dienste zu treten. Ich hoffe, du wirst meine Erwartungen nicht enttäuschen und auch die anstehenden Aufgaben bewältigen.
				name=string:DIA_DIC_Baltar_AuftragA_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7885]
		blockName=string:DIA_DIC_Baltar_Auftrag_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7886]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7887]
				subType=enum:0
				text=string:Hast du einen neuen Auftrag für mich?
				name=string:DIA_DIC_Baltar_Auftrag_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7888]
		blockName=string:DIA_DIC_Baltar_Auftrag_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7889]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7890]
				subType=enum:0
				text=string:Nein. Erst in ein paar Tagen. Einige Nachforschungen müssen noch abgeschlossen werden, bevor du mir in einer Angelegenheit von höchster Wichtigkeit helfen wirst.
				name=string:DIA_DIC_Baltar_Auftrag_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7891]
		blockName=string:DIA_DIC_Baltar_Auftrag_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7892]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7893]
				subType=enum:0
				text=string:Welche Nachforschungen?
				name=string:DIA_DIC_Baltar_Auftrag_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7894]
		blockName=string:DIA_DIC_Baltar_Auftrag_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7895]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7896]
				subType=enum:0
				text=string:Vor langer Zeit lebten hier, in Etain die zwei Magier-Orden Aciadan und Madrimier. Die Aciadan schlossen sich Madrimier an und zusammen gründeten sie ein Zentrum für die magischen Wissenschaften. Alle hochrangigen Magier des Landes lebten dort und verfeinerten ihr magisches Können.
				name=string:DIA_DIC_Baltar_Auftrag_05_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7897]
		blockName=string:DIA_DIC_Baltar_Auftrag_05_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7898]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7899]
				subType=enum:0
				text=string:Wenn ich jemandem erzähle, welche Kunstwerke der Spruchmagie aus den Händen der Madrimier einst hervorgingen, man würde es für ein Märchen halten. So mächtig waren die Madrimier einst, dass sie keine Bedrohung fürchteten.
				name=string:DIA_DIC_Baltar_Auftrag_05_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7900]
		blockName=string:DIA_DIC_Baltar_Auftrag_05_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7901]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7902]
				subType=enum:0
				text=string:Wer sollte schon die größte Magiervereinigung des Landes angreifen? Doch die Orks taten es.  Eines Tages, brachen sie mit geballter Kraft ein. Armeen bedeckten das Land. Und in der Schlacht überlebten nur wenige von den Magiern.
				name=string:DIA_DIC_Baltar_Auftrag_05_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7903]
		blockName=string:DIA_DIC_Baltar_Auftrag_05_06
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7904]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7905]
				subType=enum:0
				text=string:Doch während der Kampf zwischen den Parteien wütete, sind viele Schätze versteckt und vergraben worden.
				name=string:DIA_DIC_Baltar_Auftrag_05_06.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7906]
		blockName=string:DIA_DIC_Baltar_Auftrag_05_07
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7907]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7908]
				subType=enum:0
				text=string:In schwer zugänglichen Höhlen, oder in Geheimkammern liegen jetzt magische Artefakte. Und genau danach suchen wir.
				name=string:DIA_DIC_Baltar_Auftrag_05_07.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7909]
		blockName=string:DIA_DIC_Baltar_AuftragA_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7910]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7911]
				subType=enum:0
				text=string:Ardaric. Erst jetzt möchte ich dich einweihen. Denn du wirst in kurzer Zeit einen Auftrag von mir erhalten, dessen Bedeutung für uns im Lager nicht zu überschätzen ist. Es gibt hier in Etain drei Amulette von großer Wichtigkeit für mich.
				name=string:DIA_DIC_Baltar_AuftragA_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7912]
		blockName=string:DIA_DIC_Baltar_AuftragA_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7913]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7914]
				subType=enum:0
				text=string:Sie sind einst versteckt worden und alle meine bisherigen Versuche sie aufzuspüren, scheiterten. Es sind die sogenannten Triramar-Amulette. Die Triramar sind eine Gruppe von drei Amuletten, die zueinander gehören.
				name=string:DIA_DIC_Baltar_AuftragA_05_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7915]
		blockName=string:DIA_DIC_Baltar_AuftragA_05_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7916]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7917]
				subType=enum:0
				text=string:Vereint man ihre Kraft, gibt es kaum eine sterbliche Macht die ihnen Widerstand leisten könnte. Für unsere Sache sind diese Artefakte deswegen von größter Bedeutung.
				name=string:DIA_DIC_Baltar_AuftragA_05_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7918]
		blockName=string:DIA_DIC_Baltar_AuftragA_05_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7919]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7920]
				subType=enum:0
				text=string:Nur mit den Triramar, kann ich mir des Sieges über die Hauptstadt Gatyahs sicher sein und den Heuchler, der sich als König ausgibt, von seinem Thron stürzen.
				name=string:DIA_DIC_Baltar_AuftragA_05_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7921]
		blockName=string:DIA_DIC_Baltar_Magier_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7922]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7923]
				subType=enum:0
				text=string:Wenn doch die mächtigsten Magier des Landes hier nichts gegen die Orks ausrichten konnten, dann waren sie doch nicht wirklich mächtig, oder?
				name=string:DIA_DIC_Baltar_Magier_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7924]
		blockName=string:DIA_DIC_Baltar_Magier_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7925]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7926]
				subType=enum:0
				text=string:Täusche dich nicht. Die Orks hatten für diesen Angriff alle Kräfte gesammelt. Einige Rauchsäulen am Horizont, einen Tag vor der Schlacht, waren die einzigen Hinweise auf den Überraschungsangriff.
				name=string:DIA_DIC_Baltar_Magier_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7927]
		blockName=string:DIA_DIC_Baltar_Magier_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7928]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7929]
				subType=enum:0
				text=string:Es gab zwar Einzelne unter den Magiern die den Angriff vorausgesehen hatten, doch schenkte man ihnen keinen Glauben. Dass die Magier völlig überrascht wurden, war der große Vorteil der Orks. Ohne ihn hätten sie die Schlacht nicht gewonnen.
				name=string:DIA_DIC_Baltar_Magier_05_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7930]
		blockName=string:DIA_DIC_Baltar_Magier_15_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7931]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7932]
				subType=enum:0
				text=string:Dennoch, die Magier haben verloren. Dies wirft kein gutes Licht auf sie.
				name=string:DIA_DIC_Baltar_Magier_15_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7933]
		blockName=string:DIA_DIC_Baltar_Magier_05_04
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7934]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7935]
				subType=enum:0
				text=string:Hätten sie wirklich verloren, wäre Gatyah jetzt in der Hand der Orks. Im Gegenteil! Die Magier brachten den Ork-Armeen so hohe Verluste bei, dass die Orks ihr eigentliches Ziel aufgeben mussten.
				name=string:DIA_DIC_Baltar_Magier_05_04.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7936]
		blockName=string:DIA_DIC_Baltar_Magier_05_05
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7937]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7938]
				subType=enum:0
				text=string:Denn sie zogen nun geschwächt gegen die Hauptstadt und unterlagen schließlich den Rittern des Königs. Also unterschätze nicht den Einsatz der Magier, denen vor Jahrhunderten kein Opfer zu gering war, um den Orks den größtmöglichsten Schaden zuzufügen.
				name=string:DIA_DIC_Baltar_Magier_05_05.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7939]
		blockName=string:DIA_DIC_Baltar_Amulette_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7940]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7941]
				subType=enum:0
				text=string:Dann soll ich die Amulette finden?
				name=string:DIA_DIC_Baltar_Amulette_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7942]
		blockName=string:DIA_DIC_Baltar_Amulette_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7943]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7944]
				subType=enum:0
				text=string:Das wird deine Aufgabe sein. Und ich spüre eine magische Kraft in dir, die dir helfen wird diesen Auftrag auszuführen. Du bist wahrscheinlich besser als irgendjemand anderes im Lager dafür geeignet.
				name=string:DIA_DIC_Baltar_Amulette_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7945]
		blockName=string:DIA_DIC_Baltar_Amulette_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7946]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7947]
				subType=enum:0
				text=string:Wann kann ich anfangen?
				name=string:DIA_DIC_Baltar_Amulette_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7948]
		blockName=string:DIA_DIC_Baltar_Amulette_05_03
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7949]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7950]
				subType=enum:0
				text=string:Geduld. Graveyarn wird dich rufen lassen, wenn es soweit ist.
				name=string:DIA_DIC_Baltar_Amulette_05_03.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7951]
		blockName=string:DIA_DIC_Baltar_Verlieren_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7952]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7953]
				subType=enum:0
				text=string:Heißt das, dass ein Kampf gegen den König, ohne die Artefakte schon verloren wäre.
				name=string:DIA_DIC_Baltar_Verlieren_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7954]
		blockName=string:DIA_DIC_Baltar_Verlieren_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7955]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7956]
				subType=enum:0
				text=string:Nein. Wir sind in der Lage den König auch ohne die Hilfe der Amulette zu besiegen. Doch geben uns die Triramar-Artefakte die Gewissheit, dass wir gewinnen, bevor wir in die Schlacht ziehen.
				name=string:DIA_DIC_Baltar_Verlieren_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7957]
		blockName=string:DIA_DIC_Baltar_Welche_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7958]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7959]
				subType=enum:0
				text=string:Was sind das für Amulette?
				name=string:DIA_DIC_Baltar_Welche_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7960]
		blockName=string:DIA_DIC_Baltar_Welche_05_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7961]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 7962]
				subType=enum:0
				text=string:Damals wurden die Amulette von den Führern des Ordens geschaffen. In ihnen ist ein Teil ihrer Kraft gespeichert.
				name=string:DIA_DIC_Baltar_Welche_05_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 7963]
		blockName=string:DIA_DIC_Baltar_Welche_05_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 7964]
