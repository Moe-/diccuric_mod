		subBlock0=float:0
		[% zCCSAtomicBlock 0 8453]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8454]
				subType=enum:0
				text=string:Hast du zufällig diesen merkwürdigen Alten außerhalb des Lagers gesehen?
				name=string:DIA_DIC_Alwin_Barem_01_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8455]
