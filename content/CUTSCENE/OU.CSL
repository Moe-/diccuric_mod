		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 9989]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 9990]
				subType=enum:0
				text=string:Ich gehe jetzt in die Stadt. Du wirst mich schon finden.
				name=string:DIA_DIC_Raffa_08_14.WAV
