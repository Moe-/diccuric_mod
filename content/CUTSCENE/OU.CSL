	[]
	[% zCCSBlock 0 8668]
		blockName=string:DIA_DIC_Alwin_KAufBaltar_01_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8669]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8670]
				subType=enum:0
				text=string:Bedenke dass Berengar bald das gesamte Lager in eine Schlacht gegen den König führt. Berengar hat keine Zeit weil er unseren Überfall plant und du solltest darüber froh sein:
				name=string:DIA_DIC_Alwin_KAufBaltar_01_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8671]
		blockName=string:DIA_DIC_Alwin_KAufBaltar_01_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8672]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8673]
				subType=enum:0
				text=string:Oder willst du etwa bei einem Angriff dabei sein, der den sicheren Untergang bedeutet, nur weil der Kampfplan nicht ausgefeilt war?
				name=string:DIA_DIC_Alwin_KAufBaltar_01_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8674]
		blockName=string:DIA_DIC_Alwin_Wappen_01_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8675]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8676]
				subType=enum:0
				text=string:Gut dass ich dich treffe, Ardaric. Ich werde bald das Lager für mehrere Wochen verlassen. Graveyarn hat mich für eine Außenmission eingeteilt. Wir werden uns erst mal nicht mehr sehen. Ich wollte mich verabschieden. Man kann ja nie wissen.
				name=string:DIA_DIC_Alwin_Wappen_01_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8677]
		blockName=string:DIA_DIC_Alwin_Schild_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8678]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8679]
				subType=enum:0
				text=string:Unter den Waffen auf dem Marktplatz, war ein Schild. Ein ähnliches sah ich im Haus meines Vaters. Was waren das noch für Gerüchte, von denen du sprachst?
				name=string:DIA_DIC_Alwin_Schild_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8680]
		blockName=string:DIA_DIC_Alwin_Schild_01_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8681]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8682]
				subType=enum:0
				text=string:Ja. Dein Vater. Du arbeitest doch jetzt für Berengar. Das ist eine große Ehre. Du solltest dich entscheiden. Dein Vater ist tot, das ist wahr. Und wenn du versuchst, mehr über diesen Tod herauszufinden, läufst du vielleicht Gefahr, das Schicksal deines Vaters zu teilen.
				name=string:DIA_DIC_Alwin_Schild_01_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8683]
		blockName=string:DIA_DIC_Alwin_Schild_01_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8684]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8685]
				subType=enum:0
				text=string:Also bedenke deine Schritte gut. Ich bin mir nicht mehr sicher, ob es gut war, dir zu raten, den Gerüchten über deinen Vater nachzugehen.
				name=string:DIA_DIC_Alwin_Schild_01_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8686]
		blockName=string:DIA_DIC_Alwin_GeheAuch_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8687]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8688]
				subType=enum:0
				text=string:Auch ich verlasse das Lager, aber nur für kurze Zeit.  Ich mache mich auf Schatzsuche. In den Höhlen hier in der Umgebung soll es ja eine Menge zu finden geben.
				name=string:DIA_DIC_Alwin_GeheAuch_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8689]
		blockName=string:DIA_DIC_Alwin_GeheAuch_01_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8690]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8691]
				subType=enum:0
				text=string:Du riskierst dabei dein Leben. In den Gebirgshöhlen lauern Geschöpfe deren Namen du nicht einmal kennst. Von den Wölfen will ich gar nicht reden. Es gab hier schon ein paar wagemutige Einzelgänger, die versucht haben, wertvolles aufzustöbern, doch niemand von ihnen kehrte zurück.
				name=string:DIA_DIC_Alwin_GeheAuch_01_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8692]
		blockName=string:DIA_DIC_Alwin_GeheAuch_01_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8693]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8694]
				subType=enum:0
				text=string:Von zwei Leuten fand man die Überreste. Die meisten ihrer Knochen waren zersplittert. Das Tier das sie so zugerichtet hat, möchte ich nicht begegnen.
				name=string:DIA_DIC_Alwin_GeheAuch_01_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8695]
		blockName=string:DIA_DIC_Alwin_WohinGehst_15_00
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8696]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8697]
				subType=enum:0
				text=string:Wohin gehst du?
				name=string:DIA_DIC_Alwin_WohinGehst_15_00.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8698]
		blockName=string:DIA_DIC_Alwin_WohinGehst_01_01
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8699]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8700]
				subType=enum:0
				text=string:Ich arbeite jetzt als Späher. Zu sechst werden wir von den östlichen Gebieten eine Karte anfertigen. Begehbare Wege aufzeichnen, Landstriche vermerken die von den Wölfen bevölkert werden. Und allgemein halten wir Ausschau nach Eindringlingen jeder Art, Orks oder Menschen.
				name=string:DIA_DIC_Alwin_WohinGehst_01_01.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8701]
		blockName=string:DIA_DIC_Alwin_WohinGehst_15_02
		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8702]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8703]
				subType=enum:0
				text=string:Viel Glück. Ich hoffe wir sehen uns wieder.
				name=string:DIA_DIC_Alwin_WohinGehst_15_02.WAV
			[]
		[]
	[]
	[% zCCSBlock 0 8704]
