		numOfBlocks=int:1
		subBlock0=float:0
		[% zCCSAtomicBlock 0 8411]
			[% oCMsgConversation:oCNpcMessage:zCEventMessage 0 8412]
				subType=enum:0
				text=string:Aber wenn ich das Buch beschaffe, hilfst du mir, oder?
				name=string:DIA_DIC_Rhetorik_15_04.WAV
